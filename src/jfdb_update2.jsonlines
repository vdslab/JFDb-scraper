{"id": "380", "タイトル": "北の零年", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/bc/b2/63/e3/ca93e1142f02c285ddad7a16d1c0cda8_640.jpg", "caption": "(c) 2005「北の零年」製作委員会"}, {"url": "//jfdb.jp/data/photo/movie/bc/b2/63/e3/2ee32b4e75693f77d33bbbd464288477_640.jpg", "caption": "(c) 2005「北の零年」製作委員会"}], "公開年月日": "2005年1月15日", "上映時間": "168 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "35mm", "スタッフ": [{"name": "行定勲", "name_id": "444", "職種": "監督"}, {"名前": "岡田裕介", "id": "2990", "職種": "プロデューサー", "役割": ""}, {"名前": "那須真知子", "id": "2100", "職種": "スタッフ", "役割": "脚本"}, {"名前": "大島ミチル", "id": "668", "職種": "スタッフ", "役割": "音楽"}, {"名前": "北信康", "id": "943", "職種": "スタッフ", "役割": "撮影"}], "出演者": [{"名前": "吉永小百合", "id": "2991", "役割": "小松原志乃"}, {"名前": "渡辺謙", "id": "488", "役割": "小松原英明"}, {"名前": "豊川悦司", "id": "5620", "役割": "アシリカ"}], "製作会社": ["「北の零年」製作委員会"], "制作会社": [], "配給会社（国内）": [], "説明": "小松原志乃(吉永小百合)と夫の英明(渡辺謙)は藩での騒動がもとで、政府から北海道への移住を命じられる。家族はここで新たな生活を築こうとするのだが、寒さの厳しいこの土地では作物が育たない。そこで、英明は最新の農業技術を学ぶため、ひとり旅立つ。だが家族がいくら待っても、英明は帰ってこない。数年後、アメリカ人の指導を受けて、彼女は牧場を営んでいた。そんな彼女をアイヌと暮らす男、アシリカ(豊川悦司)が親身になって助け、やがてふたりの間には家族にも似た愛情が芽生えていく。そんな中、政府から農作業に欠かすことのできない馬を出すように命じられる。取立てにやってきたのは、すでに他の女性と結婚した英明だった。アリシカの助けもあって、無事馬を取り返した志乃はひとり、この土地で生きていくことを選ぶ。『世界の中心で、愛をさけぶ』の行定勲が監督、北海道の大自然を舞台に明治期の生活を再現している。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "379", "タイトル": "樹の海", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/a1/63/cb/04/da45c6faace24ef8ab88033297933e1e_640.jpg", "caption": "(c)「樹の海」製作委員会"}, {"url": "//jfdb.jp/data/photo/movie/a1/63/cb/04/5e799f681b65eaa022dd6b9f58905711_640.jpg", "caption": "(c)「樹の海」製作委員会"}], "公開年月日": "2005年6月25日", "上映時間": "119 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "35mm", "スタッフ": [{"name": "瀧本智行", "name_id": "5616", "職種": "監督"}, {"名前": "青島武", "id": "854", "職種": "プロデューサー", "役割": ""}, {"名前": "永田芳弘", "id": "2985", "職種": "プロデューサー", "役割": ""}, {"名前": "青島武", "id": "854", "職種": "スタッフ", "役割": "脚本"}, {"名前": "瀧本智行", "id": "5616", "職種": "スタッフ", "役割": "脚本"}, {"名前": "柴主高秀", "id": "273", "職種": "スタッフ", "役割": "撮影"}, {"名前": "吉川忠英", "id": "2987", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "萩原聖人", "id": "2206", "役割": "朝倉"}, {"名前": "井川遥", "id": "2988", "役割": "映子"}, {"名前": "池内博之", "id": "1160", "役割": "タツヤ"}], "製作会社": ["シー・アイ・エー", "ハピネット", "メモリーテック"], "制作会社": [], "配給会社（国内）": [], "説明": "自殺の名所として知られる富士山麓の青木ヶ原樹海を舞台に、「死」に直面した人々がそこから「生」を見出していく様を4つのエピソードで展開。暴力団組織にそそのかされて5億円もの公金を横領した朝倉(萩原聖人)は、口封じのために暴行を受け、樹海に遺棄されてしまう。一命を取り留めた彼は、今まさに首を吊ろうとしているひとりの男に出くわす。「止めないで下さい!」と叫ぶ男の悲痛な面もちに、朝倉は怖れをなして逃げてしまうが、森を彷徨ううちに再び同じ場所に行き着き、さっきの男が死体となった姿を目にする。朝倉はその死体に自分の想いを語り、その男が死に至るまでの苦悩や葛藤を知ることで、生き続けることを選ぶ。その他、夜逃げした顧客の足跡を追って樹海に入った金融業者のチンピラ、過去の恋愛の痛手に苦悩し、死を選ぼうとする女、そして自殺した少女の人生と死の真相を追い続ける探偵など、複数の「生と死」を巡るドラマが描かれる。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "378", "タイトル": "空中庭園", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/b3/68/6f/9f/c8604eb27579fddcfeb6c4e2b36a9809_640.jpg", "caption": "(c) 2005「空中庭園」製作委員会"}, {"url": "//jfdb.jp/data/photo/movie/b3/68/6f/9f/49a7bd4f297190edf39bfc4c02725f85_640.jpg", "caption": "(c) 2005「空中庭園」製作委員会"}], "公開年月日": "2005年11月8日", "上映時間": "113 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "35mm", "スタッフ": [{"name": "豊田利晃", "name_id": "348", "職種": "監督"}, {"名前": "菊地美世志", "id": "1886", "職種": "プロデューサー", "役割": ""}, {"名前": "豊田利晃", "id": "348", "職種": "スタッフ", "役割": "脚本"}, {"名前": "角田光代", "id": "2983", "職種": "スタッフ", "役割": "原作"}, {"名前": "藤澤順一 (J.S.C)", "id": "305", "職種": "スタッフ", "役割": "撮影"}], "出演者": [{"名前": "小泉今日子", "id": "1659", "役割": "京橋絵里子"}, {"名前": "鈴木杏", "id": "5017", "役割": "京橋マナ"}, {"名前": "板尾創路", "id": "1694", "役割": "京橋貴史"}], "製作会社": ["「空中庭園」製作委員会", "ポニーキャニオン", "衛星劇場", "朝日放送", "カルチュア・パブリッシャーズ", "アスミック・エース・エンタテインメント", "リトルモア"], "制作会社": [], "配給会社（国内）": [], "説明": "直木賞作家・角田光代の同名小説を『青い春』の豊田利晃監督が映画化。かつては華やかな新興住宅地だったが、今では「ダンチ」と呼ばれる郊外の集合住宅。母親の絵里子(小泉今日子)は理想の家族をつくるためにひとつのルールを設けていた。それは「家族の中に秘密は持たない」ということ。隠し事のないおおらかな家庭で、父親の貴史(板尾創路)、長女のマナ、弟のコウとともに暮らしていたが、実は家族のそれぞれが秘密を隠していた。娘と息子の不登校、父親の浮気。その浮気相手がコウの家庭教師としてやってきて、秘密は暴露されてしまう。一番大きな秘密を抱えていたのは絵里子だった。明るくふるまっていた彼女は暗い過去を持っていた。秘密の暴露とともに危機を迎えた一家だったが、逆にそのことが新たな家族をつくり直すきっかけとなる。ダンチの一室でひとり待つ絵里子のもとに、家族が帰ってくる。その日は絵里子の誕生日だった。", "公式サイト": [], "映画祭・受賞歴": []}
