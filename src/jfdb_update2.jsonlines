{"id": "159", "タイトル": "珈琲時光", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/c0/3f/37/e0/7c357e30c37355550d5970a9d3c54e00_640.jpg", "caption": "(C)2003 松竹/朝日新聞社/住友商事/衛星劇場/IMAGICA"}, {"url": "//jfdb.jp/data/photo/movie/c0/3f/37/e0/3fcbe5049d88ef80eaa89f39b867a27c_640.jpg", "caption": "(C)2003 松竹/朝日新聞社/住友商事/衛星劇場/IMAGICA"}], "公開年月日": "2004年9月11日", "上映時間": "108 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "35mm", "スタッフ": [{"name": "候孝賢", "name_id": "1456", "職種": "監督"}, {"名前": "宮島秀司", "id": "158", "職種": "プロデューサー", "役割": ""}, {"名前": "廖慶松", "id": "1458", "職種": "プロデューサー", "役割": ""}, {"名前": "山本一郎", "id": "1260", "職種": "プロデューサー", "役割": ""}, {"名前": "小坂史子", "id": "1454", "職種": "プロデューサー", "役割": ""}, {"名前": "候孝賢", "id": "1456", "職種": "スタッフ", "役割": "脚本"}, {"名前": "朱天文", "id": "1457", "職種": "スタッフ", "役割": "脚本"}, {"名前": "廖慶松", "id": "1458", "職種": "スタッフ", "役割": "編集"}, {"名前": "李屏賓", "id": "1459", "職種": "スタッフ", "役割": "撮影"}], "出演者": [{"名前": "一青窈", "id": "1460", "役割": "陽子"}, {"名前": "浅野忠信", "id": "37", "役割": "肇"}, {"名前": "小林稔侍", "id": "489", "役割": "陽子の父"}], "製作会社": ["松竹", "朝日新聞社", "住友商事", "衛星劇場", "IMAGICA"], "制作会社": [], "配給会社（国内）": [], "説明": "小津安二郎生誕100年を記念し、『悲情城市』『憂鬱な楽園』などで知られる台湾の監督、侯孝賢が東京を舞台に撮った作品。都電が走る住宅街。フリーライターの陽子(一青窈)は昨夜台湾から戻ってきたばかりだ。友人の肇(浅野忠信)に土産を渡した後、陽子はお墓参りのため高崎の実家に帰省する。彼女は久しぶりに再会した両親に自分が妊娠していることを告げる。両親はとまどいを隠せない。再び東京に戻った陽子からその事実を知らされた肇も動揺を見せる。肇は彼女に好意を寄せていたのだ。しかし、肇は体調を崩した陽子を優しく見守り、コンピューター上で描いた電車の絵を見せて彼女を和ませる。陽子を心配した両親が上京してくるが、彼女は相手の台湾人男性と結婚する気はなく、シングルマザーの道を選ぶという。陽子は電車に揺られながら、家族、肇、生まれてくる子供のことを静かに想うのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "158", "タイトル": "AIKI", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/47/3d/48/e8/f7552210ee0fbda3389a292d50d43c3b_640.jpg", "caption": "(c) 2002 「AIKI」製作委員会"}, {"url": "//jfdb.jp/data/photo/movie/47/3d/48/e8/f61db1d655a9608cf1ad0e37b71928ae_640.jpg", "caption": "(c) 2002 「AIKI」製作委員会"}], "公開年月日": "2002年11月30日", "上映時間": "119 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "天願大介", "name_id": "1446", "職種": "監督"}, {"名前": "飯野久", "id": "1443", "職種": "プロデューサー", "役割": ""}, {"名前": "松田康史", "id": "1444", "職種": "プロデューサー", "役割": ""}, {"名前": "古賀俊輔", "id": "1445", "職種": "プロデューサー", "役割": ""}, {"名前": "天願大介", "id": "1446", "職種": "スタッフ", "役割": "脚本"}, {"名前": "李以須", "id": "1447", "職種": "スタッフ", "役割": "撮影"}, {"名前": "めいな co.", "id": "5354", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "加藤晴彦", "id": "1448", "役割": "芦原太一"}, {"名前": "ともさかりえ", "id": "1137", "役割": "サマ子"}, {"名前": "石橋凌", "id": "5579", "役割": "平石正嗣"}], "製作会社": ["日活", "バップ", "衛星劇場", "広美", "IMAGICA", "レントラックジャパン", "カプリースブリッジ"], "制作会社": [], "配給会社（国内）": [], "説明": "監督の天願大輔は今村昌平の実子であり、『うなぎ』以降の今村作品の脚本にも参加している。新人王を目指すボクサー・芦原太一(加藤晴彦)はバイク事故にあい下半身不随になる。夢を断たれ、仕事も見つからず人生の目標も定まらない。そんな中で偶然、太一はテキ屋の仕事を紹介される。彼は縁日の店先でサマ子(ともさかりえ)に出会う。再び打ち込める何かを探し始めた太一はさまざまな格闘技の道場を訪ねるが、その先々で断られる。ある日サマ子を訪ねていった神社の境内で太一は合気柔術に魅了され、そんな彼を師範の平石(石橋凌)は温かく受け入れる。車椅子を自分の身体の一部と考えるようになった太一は徐々に上達していく。だが突然、サマ子はもめ事に巻き込まれ姿を消す。とまどう太一のもとに、近々大使館で行われる演武会に出てみないかという話が平石から持ちかけられる。演武会で見事な技を披露し、確かな手ごたえを感じた太一は、サマ子を探す旅に出ることを決意する。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "157", "タイトル": "Dolls", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/6f/94/b9/63/d15534a12098dde2c97118152ea294ea_640.jpg", "caption": "(c) 2002 Bandai Visual, Tokyo FM, Television Tokyo and Office Kitano"}, {"url": "//jfdb.jp/data/photo/movie/6f/94/b9/63/4b378511d42575dbad527d6c25e74996_640.jpg", "caption": "(c) 2002 Bandai Visual, Tokyo FM, Television Tokyo and Office Kitano"}], "公開年月日": "2002年10月12日", "上映時間": "113 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "北野武", "name_id": "627", "職種": "監督"}, {"名前": "森昌行", "id": "5010", "職種": "プロデューサー", "役割": ""}, {"名前": "吉田多喜男", "id": "1245", "職種": "プロデューサー", "役割": ""}, {"名前": "北野武", "id": "627", "職種": "スタッフ", "役割": "脚本"}, {"名前": "柳島克己", "id": "629", "職種": "スタッフ", "役割": "撮影"}, {"名前": "久石譲", "id": "164", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "菅野美穂", "id": "1439", "役割": "佐和子"}, {"名前": "西島秀俊", "id": "1440", "役割": "松本"}, {"名前": "三橋達也", "id": "1441", "役割": "親分"}], "製作会社": ["バンダイビジュアル", "エフエム東京", "テレビ東京", "オフィス北野"], "制作会社": [], "配給会社（国内）": [], "説明": "北野武監督の最新作。「つながり乞食」と呼ばれる、互いの身体を赤い紐で結んで歩く男女の悲しい道行きを、近松門佐衛門の「冥土の飛脚」をモチーフに描き、そこにふたつの異なるラヴストーリーが挿入される。かつては人も羨むカップルだった松本(西島秀俊)と佐和子(菅野美穂)。二人は結婚の約束もしていたが、社長令嬢との縁談が舞い込んだ松本は佐和子を捨てた。彼女は自殺未遂の末に精神のバランスを崩し、過去の記憶を全て失ってしまう。結婚式当日、そのことを知った松本は式場を抜け出し病院から佐和子を連れ出す。彼は、目を離すとどこへ行くかわからない佐和子と自分の身体を赤い紐で結ぶ。車を乗り捨て行くあてもなく歩き出した二人は、道すがら、変わり行く季節の中で様々な人とすれ違う。季節は冬となり、一面の雪の中、足を滑らせた二人は崖から転落する。二人をつなぐ紐が崖の中腹にはえた枝に引っ掛かり、宙吊りとなったまま旅は終わる。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "156", "タイトル": "DRIVE", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/7d/54/a4/6d/368962a18af8dc71a685725a413ae733_640.jpg", "caption": "(c) 2001 DRIVE Film Partners"}, {"url": "//jfdb.jp/data/photo/movie/7d/54/a4/6d/9f9b61fe863cd64e2a21a243291a36a8_640.jpg", "caption": "(c) 2001 DRIVE Film Partners"}], "公開年月日": "2002年8月24日", "上映時間": "102 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "SABU", "name_id": "687", "職種": "監督"}, {"名前": "久保田修", "id": "112", "職種": "プロデューサー", "役割": ""}, {"名前": "平野隆", "id": "5284", "職種": "プロデューサー", "役割": ""}, {"名前": "SABU", "id": "687", "職種": "スタッフ", "役割": "脚本"}, {"名前": "佐藤和人", "id": "1433", "職種": "スタッフ", "役割": "撮影"}, {"名前": "村瀬恭久", "id": "689", "職種": "スタッフ", "役割": "音楽"}, {"名前": "坂東俊秀", "id": "1434", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "堤真一", "id": "4760", "役割": "朝倉健一"}, {"名前": "大杉漣", "id": "1436", "役割": "西五郎"}, {"名前": "柴咲コウ", "id": "1544", "役割": "坂井スミレ"}], "製作会社": ["IMJエンタテインメント", "TBS", "毎日放送", "エフエム東京", "広美", "パイオニアLDC", "日本ヘラルド映画"], "制作会社": [], "配給会社（国内）": [], "説明": "朝倉(堤真一)は、極端に几帳面な性格のせいで絶えず頭痛を抱えている。いつも通りかかる花屋の前にいた女性(柴崎コウ)に見とれていると、覆面の3人組が車に乗り込んできて、前の車を追え、と脅される。男達は銀行強盗で、仲間の一人が金を持ち逃げしたのだ。が、制限速度ぴったりで走る朝倉の運転では、裏切り者に追いつくことは出来ない。結局朝倉は、強盗達と共に逃げるはめになるのだが、その途中、強盗達はそれぞれの目的を見つけて去っていく。ひとり車に残った朝倉。携帯電話が鳴り、今度は金を持ち逃げした男に助けを求められる。その場所に向かった朝倉を待っていたのは、彼の先祖の亡霊だった。朝倉との勝負に負けて先祖の霊は成仏する。そして後日、銀行に強奪金を返しにいった彼は、そこで例の花屋の前の女性が働いていることを知る……。『MONDAY』で2000年ベルリン映画祭国際批評家連盟賞を受賞したSABUが、独自の疾走感とともに描く、人生のドライヴ。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "155", "タイトル": "KT", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/37/ca/0b/c4/61c1a13b83b67f1b047b3ba8af90d893_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/37/ca/0b/c4/4c68214fc21c74274514f35a16244a9f_640.jpg", "caption": ""}], "公開年月日": "2002年5月3日", "上映時間": "138 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "阪本順治", "name_id": "212", "職種": "監督"}, {"名前": "李鳳宇", "id": "608", "職種": "プロデューサー", "役割": ""}, {"名前": "椎井友紀子", "id": "1425", "職種": "プロデューサー", "役割": ""}, {"名前": "荒井晴彦", "id": "857", "職種": "スタッフ", "役割": "脚本"}, {"名前": "中薗英助", "id": "1426", "職種": "スタッフ", "役割": "原作"}, {"名前": "笠松則通", "id": "215", "職種": "スタッフ", "役割": "撮影"}, {"名前": "布袋寅泰", "id": "1427", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "佐藤浩市", "id": "166", "役割": "富田満州男"}, {"名前": "キム・ガプス", "id": "1428", "役割": "金車雲"}, {"名前": "原田芳雄", "id": "350", "役割": "神川昭和"}], "製作会社": ["KT製作委員会"], "制作会社": [], "配給会社（国内）": [], "説明": "73年に起こった「金大中拉致事件」。日本全国に衝撃を与え、日韓関係を揺るがしたこの大事件が、大胆な推測と細やかな人間描写によって、国家的規模の陰謀と個人的な人生の歩みとが交錯する社会派サスペンスへと変貌した。主人公は金大中の誘拐に関与した自衛官、富田(佐藤浩市)。事件は彼の行動を追いながら描かれていく。事件の流れと共に、北朝鮮のスパイでしかし富田と心を通わせることになる李政美(ヤン・ウニョン)、韓国大使館一等書記官で富田に隠密行動中の金大中の捜索を依頼する金車雲(キム・ガプス)、金大中とのインタビューに成功したゴシップ新聞記者の神川(原田芳雄)、日本人ガールフレンドとの関係に苦しむ金大中のボディガード金甲寿(筒井道隆)などなど、事件の背景にいる人々の生活と夢が、浮かび上がる。しかしアメリカとの関係の中でも決断が下される国家戦略が、彼らの人生の前に大きく立ちはだかる。それぞれの思惑や交流を超えて、事件は進行していく。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "154", "タイトル": "OUT", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/e0/d6/bb/67/86021d406ca16d5bce7fc7383f1c2b09_640.jpg", "caption": "(c) 「OUT」製作委員会 サンダンス・カンパニー"}, {"url": "//jfdb.jp/data/photo/movie/e0/d6/bb/67/20024dac8cd177c31a2ac1536341442e_640.jpg", "caption": "(c) 「OUT」製作委員会 サンダンス・カンパニー"}], "公開年月日": "2002年10月19日", "上映時間": "119 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "平山秀幸", "name_id": "172", "職種": "監督"}, {"名前": "古澤利夫", "id": "1417", "職種": "プロデューサー", "役割": ""}, {"名前": "木村典代", "id": "4839", "職種": "プロデューサー", "役割": ""}, {"名前": "中條秀勝", "id": "1415", "職種": "プロデューサー", "役割": ""}, {"名前": "藤田義則", "id": "1419", "職種": "プロデューサー", "役割": ""}, {"名前": "福島聡司", "id": "1150", "職種": "プロデューサー", "役割": ""}, {"名前": "諸橋健一", "id": "4838", "職種": "スタッフ", "役割": "製作総指揮"}, {"名前": "鄭義信", "id": "1320", "職種": "スタッフ", "役割": "脚本"}, {"名前": "桐野夏生", "id": "1420", "職種": "スタッフ", "役割": "原作"}, {"名前": "柴崎幸三", "id": "518", "職種": "スタッフ", "役割": "撮影"}, {"名前": "安川午朗", "id": "176", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "原田美枝子", "id": "1177", "役割": "香取雅子"}, {"名前": "倍賞美津子", "id": "1421", "役割": "吾妻ヨシエ"}, {"名前": "室井滋", "id": "1422", "役割": "城之内邦子"}], "製作会社": ["ムービーテレビジョン", "サンダンス・カンパニー"], "制作会社": [], "配給会社（国内）": [], "説明": "主婦たちの犯罪と彼女たちによる死体解体の生々しい描写で話題を呼び、ベストセラーとなった桐野夏生のミステリー小説が原作。雅子(原田美枝子)、ヨシエ(倍賞美津子)、邦子(室井滋)、弥生(西田尚美)は弁当工場に勤めるパート仲間。ある日雅子のもとに、夫を殺してしまったと弥生が電話をかけてきた。死体の処理を頼まれた雅子は、死体をバラバラに解体し、ゴミとして捨てることを思い付く。ヨシエにも強引に協力を頼み、二人は雅子の家で死体を解体する。そこに偶々現れた邦子も報酬に釣られ加担するが、彼女のミスで死体の一部が発見されてしまう。しかも邦子は弥生を脅迫し、借金の保証人にしていた。それをきっかけに事件を知った金融業者の十文字(香川照之)が、雅子たちに死体処理のビジネスを持ちかけてきた。再び死体を解体することになる三人。その後弥生のもとに切断された十文字の手首が届けられる。夫殺しの罪を被せられた男が復習を始めたのだ……。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "153", "タイトル": "SABU 〜さぶ〜", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/53/0e/03/fd/18889926b76a570d88fccfca0eb9a018_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/53/0e/03/fd/2ec21eb28f8d96ebe8256b8b2d8dce89_640.jpg", "caption": ""}], "公開年月日": "2002年10月5日", "上映時間": "121 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "三池崇史", "name_id": "123", "職種": "監督"}, {"名前": "原口淳", "id": "1409", "職種": "プロデューサー", "役割": ""}, {"名前": "菅原章", "id": "1410", "職種": "プロデューサー", "役割": ""}, {"名前": "吉田浩二", "id": "1341", "職種": "プロデューサー", "役割": ""}, {"名前": "竹山洋", "id": "1411", "職種": "スタッフ", "役割": "脚本"}, {"名前": "山本周五郎", "id": "1373", "職種": "スタッフ", "役割": "原作"}, {"名前": "山本英夫 (J.S.C.)", "id": "81", "職種": "スタッフ", "役割": "撮影"}, {"名前": "遠藤浩二", "id": "126", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "藤原竜也", "id": "307", "役割": "栄ニ"}, {"名前": "妻夫木聡", "id": "5387", "役割": "さぶ"}, {"名前": "田畑智子", "id": "1412", "役割": "おのぶ"}], "製作会社": ["名古屋テレビ放送", "電通", "セディックインターナショナル"], "制作会社": [], "配給会社（国内）": [], "説明": "かつて今村昌平や恩地日出夫の助監督を務め、1995年のデビュー以来『DEADORALIVE』『殺し屋1』などジャンルを問わず手腕を発揮し、驚異的なハイペースで作品を撮りつづけている三池崇史が放つ本格的時代劇。幼い頃から共に住み込みで働いている同い年の表具職人・栄二(藤原竜也)とさぶ(妻夫木聡)。ある日栄二はあらぬ盗みの罪をきせられ、姿を消した。栄二の行方を探すさぶ。石川島の寄場で強制労働をさせられていた栄二は、人間不信に陥り心を閉ざしていた。居場所を突き止めてやってきたさぶにも、栄二は冷たい言葉を放つ。しかし寄場の人間たちの働きかけで、栄二は少しずつ心を開く。そんな時に石川島に台風が上陸する。人足たちは避難のために7日間だけ放免されることになったのだが、栄二は役人たちと共に寄場を守ると言い出し仲間を集める。そんな栄二の働きでなんとか台風は去り、やがて堅気の世界へと帰ってきた栄二をさぶは温かく迎えた。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "152", "タイトル": "Unloved", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/98/2d/fa/58/0e21ea6eae8e5c9f1b12a27bf7806654_640.jpg", "caption": "(c) 2002 WOWOW + バンダイビジュアル"}, {"url": "//jfdb.jp/data/photo/movie/98/2d/fa/58/63df1a592fb236cd640c5e2fcc8d884d_640.jpg", "caption": "(c) 2002 WOWOW + バンダイビジュアル"}], "公開年月日": "2002年5月25日", "上映時間": "117 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "万田邦敏", "name_id": "1402", "職種": "監督"}, {"名前": "仙頭武則", "id": "420", "職種": "プロデューサー", "役割": ""}, {"名前": "万田珠実", "id": "1403", "職種": "スタッフ", "役割": "脚本"}, {"名前": "万田邦敏", "id": "1402", "職種": "スタッフ", "役割": "脚本"}, {"名前": "芦澤明子", "id": "676", "職種": "スタッフ", "役割": "撮影"}, {"名前": "川井憲次", "id": "2318", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "森口瑶子", "id": "1405", "役割": "影山光子"}, {"名前": "仲村トオル", "id": "1406", "役割": "勝野英治"}, {"名前": "松岡俊介", "id": "1407", "役割": "下川弘"}], "製作会社": ["WOWOW", "バンダイビジュアル"], "制作会社": [], "配給会社（国内）": [], "説明": "2001年のカンヌ映画祭でレイル・ドール賞とエキュメニック新人賞を受賞した、万田邦敏監督の初長編。誰もが無意識のうちに受け入れてしまっている日本社会の男性原理に対し、根源的な問いを発しつづける女性、光子(森口瑶子)と周囲の軋轢が、緻密な会話のやり取りによって描かれていく。普通の公務員として地味に暮らす光子の前に、光子とは対象的な華やかで浮き沈みの激しいビジネスの世界で生きる企業家、勝野(仲村トオル)が現れる。二人は急速に親密になり、勝野は、自分の暮らす世界に光子を引き入れようとするのだが、光子にとって結局はそれが負担となってしまう。そんな時、光子はアルバイトをしながら気ままに暮らす、下川(松岡俊介)と知り合う。光子は下川の野心のない暮らし方に惹かれたのだ。だが、勝野へのコンプレックスからか、下川もまた、気ままな暮らしから這い上がろうとする。光子はそんな下川に、「どうして今までの暮らしではいけないのか」と詰め寄っていく。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "151", "タイトル": "青い春", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/18/48/fb/4a/541574697782854d3e867bf0dc7f1e24_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/18/48/fb/4a/fbce060cbdb18cd7a6945313abb80b4a_640.jpg", "caption": ""}], "公開年月日": "2002年6月29日", "上映時間": "83 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "豊田利晃", "name_id": "348", "職種": "監督"}, {"名前": "宮崎大", "id": "240", "職種": "プロデューサー", "役割": ""}, {"名前": "小林智", "id": "1397", "職種": "プロデューサー", "役割": ""}, {"名前": "豊田利晃", "id": "348", "職種": "スタッフ", "役割": "脚本"}, {"名前": "松本大洋", "id": "1197", "職種": "スタッフ", "役割": "原作"}, {"名前": "笠松則通", "id": "215", "職種": "スタッフ", "役割": "撮影"}, {"名前": "上田ケンジ", "id": "1399", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "松田龍平", "id": "5344", "役割": "九條"}, {"名前": "新井浩文", "id": "1400", "役割": "青木"}, {"名前": "高岡蒼佑", "id": "1401", "役割": "雪男"}], "製作会社": ["ミコット", "ゼアリズエンタープライズ", "小学館", "ケイエスエス", "日活", "日販"], "制作会社": [], "配給会社（国内）": [], "説明": "阪本順治の『王手』『ビリケン』の脚本・助監督を務めた豊田利晃の3本目の長編映画。人気漫画家、松本大洋の作品の初映画化。閉塞感を抱きながら退屈な高校生活を過ごす九條(松田龍平)や幼馴染みの青木(新井浩文)ら新3年生は、卒業式の校舎の屋上で柵の外枠につかまって手を何回たたけるかというベランダゲームをしていた。この勝者が学校を仕切るという伝統のゲームで、九條は8回という歴代最高記録をたたき出す。だが彼にとってこのゲームも、学校を仕切るということも、無意味で面倒臭いことだった。彼の態度に煮え切らない思いを抱いた青木は、髪型も態度もがらりと変える。ある朝、校舎の屋上に立つ青木。彼はひとりでベランダゲームを始める。不吉な予感に駆られて屋上へ駆け上がる九條。彼が屋上のドアを開けた瞬間、青木の身体は柵から離れていった。13まで数えたところで、青木は校庭に叩き付けられた。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "150", "タイトル": "阿弥陀堂だより", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/2c/79/c9/cd/3ee9e28a3ef11e847739e86878b49b9e_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/2c/79/c9/cd/5cd9fdfb5cbc175d9bdc02acdc48012b_640.jpg", "caption": ""}], "公開年月日": "2002年10月5日", "上映時間": "128 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "小泉堯史", "name_id": "1390", "職種": "監督"}, {"名前": "柘植靖司", "id": "1387", "職種": "プロデューサー", "役割": ""}, {"名前": "荒木美也子", "id": "1388", "職種": "プロデューサー", "役割": ""}, {"名前": "桜井勉", "id": "1389", "職種": "プロデューサー", "役割": ""}, {"名前": "小泉堯史", "id": "1390", "職種": "スタッフ", "役割": "脚本"}, {"名前": "南木佳士", "id": "1391", "職種": "スタッフ", "役割": "原作"}, {"名前": "上田正治", "id": "19", "職種": "スタッフ", "役割": "撮影"}, {"名前": "加古隆", "id": "1392", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "寺尾聰", "id": "2610", "役割": "上田孝夫"}, {"名前": "樋口可南子", "id": "509", "役割": "上田美智子"}, {"名前": "北林谷栄", "id": "1394", "役割": "おうめ"}], "製作会社": ["「阿弥陀堂だより」製作委員会"], "制作会社": [], "配給会社（国内）": [], "説明": "売れない小説家の孝夫(寺尾聰)は有能な医者である妻・美智子(樋口可南子)と東京で暮らしていた。ある日、美智子がパニック障害という原因不明の心の病にかかってしまう。それをきっかけに夫婦は、孝夫の故郷、信州に移り住むことを決意。山里の村に帰った二人は、阿弥陀堂という、村の死者が祭られたお堂で暮らす老婆・おうめ(北林谷栄)を訪ねる。そこで孝夫はおうめの日々の思いを書き留め、村の広報誌に掲載している小百合(小西真奈美)と出会う。小百合は喋ることができない難病を抱えていた。無医村だった村に診療所を開いた美智子は、おうめや真奈美の診療を通して、医者としての自信を回復していく。また、夫婦は村の人々や自然との触れあいを通して、生きる喜びを取り戻すのだった。そんな時、小百合の病状が悪化。美智子は彼女の手術を担当することを決意する。黒澤明の遺稿脚本『雨あがる』を映画化した小泉尭史の監督第2作目。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "149", "タイトル": "アレクセイと泉", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/5e/7b/7e/12/e7d89a595d2018b78b010f3a7c4695ab_640.jpg", "caption": "(c) 2002 ポレポレタイムス社"}, {"url": "//jfdb.jp/data/photo/movie/5e/7b/7e/12/b40148af178668c02eba196947ea9fd6_640.jpg", "caption": "(c) 2002 ポレポレタイムス社"}], "公開年月日": "2002年1月20日", "上映時間": "104 分", "カテゴリー": ["ドキュメンタリー"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "本橋成一", "name_id": "1384", "職種": "監督"}, {"名前": "小松原時夫", "id": "569", "職種": "プロデューサー", "役割": ""}, {"名前": "神谷さだ子", "id": "1383", "職種": "プロデューサー", "役割": ""}, {"名前": "一之瀬正史", "id": "5069", "職種": "スタッフ", "役割": "撮影"}, {"名前": "坂本龍一", "id": "82", "職種": "スタッフ", "役割": "音楽"}], "出演者": [], "製作会社": ["サスナフィルム"], "制作会社": [], "配給会社（国内）": [], "説明": "写真家でもある本橋成一が、前作『ナージャの村』に引き続いてチェルノブイリ以後の世界を描いた、小さな村の泉を取り巻く人々のドキュメンタリー。豊かな原生林と肥沃で豊穰な大地を持ち、白ロシアと呼ばれるベラルーシ共和国。チェルノブイリで起こった史上最悪の原発事故によって、放射能で国土が汚染されてしまったこの国の東南部に位置する小さな村、ブジシチェ。高汚染によって移住勧告が出され、かつて600人を数えた住人も、今は55人の年寄りと一人の青年アレクセイを残すばかりである。不思議なことにこの村の中心に湧く泉からは、放射能が検出されない。老人達の代わりに、力仕事やトラクターの運転を一手に引き受けるアレクセイ。薪を割り、木を切り、じゃがいもを掘る。収穫祭では広場で料理を食べ、ウォッカを飲み、歌い踊る。酔い潰れている者もある。彼らの日常は、美しい自然に囲まれた泉を中心として、事故を経ても変わらず静かに過ぎていく。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "148", "タイトル": "命", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/e8/76/88/5b/b7091c307aefd68bbe8c98aacfa0ba91_640.jpg", "caption": "(c) 2002 TOEI COMPANY, LTD."}, {"url": "//jfdb.jp/data/photo/movie/e8/76/88/5b/971477b0dc106c21675cdedd9812b9de_640.jpg", "caption": "(c) 2002 TOEI COMPANY, LTD."}], "公開年月日": "2002年9月14日", "上映時間": "111 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "篠原哲雄", "name_id": "505", "職種": "監督"}, {"名前": "伊與田英徳", "id": "1378", "職種": "プロデューサー", "役割": ""}, {"名前": "進藤淳一", "id": "3678", "職種": "プロデューサー", "役割": ""}, {"名前": "大森寿美男", "id": "506", "職種": "スタッフ", "役割": "脚本"}, {"名前": "柳美里", "id": "1380", "職種": "スタッフ", "役割": "原作"}, {"名前": "浜田毅", "id": "163", "職種": "スタッフ", "役割": "撮影"}, {"名前": "村山達哉", "id": "508", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "江角マキコ", "id": "1381", "役割": "柳美里"}, {"名前": "豊川悦司", "id": "5620", "役割": "東由多加"}, {"名前": "筧利夫", "id": "341", "役割": "鎌田"}], "製作会社": ["「命」製作委員会"], "制作会社": [], "配給会社（国内）": [], "説明": "美里(江角マキコ)は妻子ある男性の子供を妊娠。彼も初めは子供を産むことを望んだが、今の家庭も捨てられず、煮え切らない様子。救いを求めるように、恩人であり昔の恋人でもある劇作家・東由多加のもとを訪ねた美里は、東が手の施しようのない癌に侵されていることを知る。そして美里は東とともに病気と闘うことを決意し、また、その絆から二人で子供を育てようという約束が生まれた。「わたしの中には、今、はっきりと二つの命が息づいている」と思う美里だったが、東の抗癌剤治療の効果はなかなか得られず、彼は副作用に苦しみ、日に日に衰えていく。そんな中美里はついに男の子を出産し、丈陽と名付ける。子供が自分の名前を呼べるようになるまで生きたいと、今まで以上に生きることに執念を燃やす東だったが、その願いがかなわないまま、美里と丈陽に見守られながら東は息をひきとった。原作は、作家の柳美里が自身の体験を赤裸々に綴ったベストセラー小説。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "147", "タイトル": "海は見ていた", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/27/13/80/69/3a6bbc6b41f19d9b2358bb81ccce3056_640.jpg", "caption": "(c) 2002「海は見ていた」製作委員会"}, {"url": "//jfdb.jp/data/photo/movie/27/13/80/69/168dee62340f2286af297add3a2c5ad5_640.jpg", "caption": "(c) 2002「海は見ていた」製作委員会"}], "公開年月日": "2002年7月27日", "上映時間": "119 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "熊井啓", "name_id": "1371", "職種": "監督"}, {"名前": "猿川直人", "id": "1370", "職種": "プロデューサー", "役割": ""}, {"名前": "黒澤明", "id": "1372", "職種": "スタッフ", "役割": "脚本"}, {"名前": "山本周五郎", "id": "1373", "職種": "スタッフ", "役割": "原作"}, {"名前": "奥原一男", "id": "1374", "職種": "スタッフ", "役割": "撮影"}, {"名前": "松村禎三", "id": "848", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "清水美砂", "id": "1375", "役割": "キクノ"}, {"名前": "遠野凪子", "id": "1376", "役割": "オシン"}, {"名前": "永瀬正敏", "id": "1088", "役割": "リョウスケ"}], "製作会社": ["日活"], "制作会社": [], "配給会社（国内）": [], "説明": "故・黒澤明監督の遺稿脚本を、熊井啓監督が忠実に映像化した時代恋愛劇。江戸・深川の女郎屋で働くお新(遠野凪子)は、ある夜刃傷沙汰を起こして逃げてきた若侍(吉岡秀隆)に恋をする。彼女の健気な姿を見た姉さん衆も彼女の恋を応援しようとするが、若侍はお新を妻にする気はまったくなく、姉妹のように思っていただけだった。寝込むほどに傷付いたお新のもとに、謎めいた良介(永瀬正俊)という男が現れる。同じく不幸な境遇のもと生まれた二人はお互いを想い合うようになる。そんなある日、深川一帯を大洪水が襲い、店を女将から任された菊乃(清水美砂)とお新たちは逃げ遅れる。その時菊乃のヒモである銀次(奥田瑛二)が金銭を奪いにやってきて、良介は彼を殺してしまう。良介を逃した菊乃たちは逃げ場を失い、ついに屋根まで追い詰められる。そこへ小舟に乗った良介が助けに現れるが、その舟に3人は乗れず、菊乃は自ら屋根に残ることを選ぶ。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "146", "タイトル": "折り梅", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/37/39/81/ab/d4f33be1a059b7d98b9b38c40ba4097c_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/37/39/81/ab/13a43dc62b762ca56bf00230928e7a5c_640.jpg", "caption": ""}], "公開年月日": "2002年3月16日", "上映時間": "111 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "松井久子", "name_id": "1362", "職種": "監督"}, {"名前": "松井久子", "id": "1362", "職種": "プロデューサー", "役割": ""}, {"名前": "新藤次郎", "id": "1363", "職種": "プロデューサー", "役割": ""}, {"名前": "里中哲夫", "id": "6181", "職種": "プロデューサー", "役割": ""}, {"名前": "松井久子", "id": "1362", "職種": "スタッフ", "役割": "脚本"}, {"名前": "白鳥あかね", "id": "1365", "職種": "スタッフ", "役割": "脚本"}, {"名前": "小菅もと子", "id": "1366", "職種": "スタッフ", "役割": "原作"}, {"名前": "川上皓市", "id": "718", "職種": "スタッフ", "役割": "撮影"}, {"名前": "川崎真弘", "id": "1367", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "原田美枝子", "id": "1177", "役割": "巴"}, {"名前": "吉行和子", "id": "1368", "役割": "政子"}, {"名前": "トミーズ雅", "id": "1369", "役割": "裕三"}], "製作会社": ["エッセンコミュニケーションズ"], "制作会社": [], "配給会社（国内）": [], "説明": "前作『ユキエ』で老夫婦のゆったりした生活を撮った松井久子が、本作では若い人たちの生活のリズムからはみだしてしまった老人を描く。公園で油絵を描く政子(吉行和子)とそれを見守る巴(原田美枝子)。この二人が痴呆症という病を乗り越えるまでには、長い葛藤を経なければならなかった。一人暮らしだった政子は、三男の祐三の戸建住宅に引き取られてすぐアルツハイマー病を発症する。巴はヘルパーを頼み、あるグループホームを祐三と訪れ、政子を預けることに決める。別れの当日、巴は政子の生い立ちを初めて耳にする。幼子を捨てた母が政子に教えた折り梅のこと、里子に出され、お針子として働き、子供4人を残して夫がなくなった時のこと。巴は政子を預けるのをやめ、近くの寺のデイケアに連れていく。そこで政子が始めたのが絵だった。夕陽に染まる坂道を、公園にいる政子のもとへと、巴は自転車のペダルをこぐのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "145", "タイトル": "害虫", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/32/30/03/e8/4420eacb12924a23bbaea12470c57447_640.jpg", "caption": "(c) 2002 NIKKATSU/TBS/SONY PCL"}, {"url": "//jfdb.jp/data/photo/movie/32/30/03/e8/c0a5a3f7c0862e3ac84ba3a1298dea60_640.jpg", "caption": "(c) 2002 NIKKATSU/TBS/SONY PCL"}], "公開年月日": "2002年3月16日", "上映時間": "92 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "塩田明彦", "name_id": "113", "職種": "監督"}, {"名前": "根岸洋之", "id": "1179", "職種": "プロデューサー", "役割": ""}, {"名前": "平野隆", "id": "5284", "職種": "プロデューサー", "役割": ""}, {"名前": "清野弥生", "id": "1358", "職種": "スタッフ", "役割": "脚本"}, {"名前": "喜久村徳章", "id": "116", "職種": "スタッフ", "役割": "撮影"}, {"名前": "ナンバーガール", "id": "1359", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "宮﨑あおい", "id": "1360", "役割": "北サチ子"}, {"名前": "田辺誠一", "id": "79", "役割": "緒方智"}, {"名前": "りょう", "id": "1111", "役割": "北稔子"}], "製作会社": ["日活", "TBS", "ソニーPCL"], "制作会社": [], "配給会社（国内）": [], "説明": "『EUREKA』の少女役で注目を浴びた宮崎あおいが、過酷な運命の中で周囲から孤立しながらも果敢に自らの生きる道を選び取る女子中学生を演じる。2001年ナント三大陸映画祭で審査員特別賞、主演女優賞を受賞した。母の自殺騒動の影響で同級生から浮いた存在になってしまったサチ子(宮崎あおい)は、学校へは行かず街へ出て、気ままな時間を過ごしている。ある日サチ子は、タカオ(沢木哲)と浮浪者キュウゾウ(石川浩司)に出会う。3人は特に何をするわけでもないが、それゆえかけがえのない緩やかな時間の流れを体験する。その中で、サチ子と小学校時代の担任、緒方との恋愛関係も明らかになる。しかし、トラブルに巻き込まれたタカオが姿を消し、サチ子たちの緩やかな時間の流れは突然断ち切られる。サチ子は再び学校へと通い始めるが、そこでもトラブルが待ち受けている。サチ子は街を離れることを決断。緒方を訪ねるため、長距離トラックに乗り込んで行くのだが……。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "144", "タイトル": "火星のカノン", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/fb/04/5f/81/778e89826bc3a2bb97408717edaa5039_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/fb/04/5f/81/a225eb74478dac7bbd8ad44546cf6fb2_640.jpg", "caption": ""}], "公開年月日": "2002年9月28日", "上映時間": "121 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "風間志織", "name_id": "1349", "職種": "監督"}, {"名前": "伊藤直克", "id": "559", "職種": "プロデューサー", "役割": ""}, {"名前": "岡本東郎", "id": "1301", "職種": "プロデューサー", "役割": ""}, {"名前": "斎藤緑", "id": "1348", "職種": "プロデューサー", "役割": ""}, {"名前": "小川智子", "id": "1350", "職種": "スタッフ", "役割": "脚本"}, {"名前": "及川章太郎", "id": "1351", "職種": "スタッフ", "役割": "脚本"}, {"名前": "石井勲", "id": "1352", "職種": "スタッフ", "役割": "撮影"}, {"名前": "阿部正也", "id": "1353", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "久野真紀子", "id": "1354", "役割": "絹子"}, {"名前": "小日向文世", "id": "1355", "役割": "公平"}, {"名前": "中村麻美", "id": "2850", "役割": "聖"}], "製作会社": ["アルゴ・ピクチャーズ", "バップ", "アンフィニー"], "制作会社": [], "配給会社（国内）": [], "説明": "『冬の河童』で94年ロッテルダム国際映画祭のタイガーアワードを受賞した風間志織監督の、7年ぶりになる長編。妻子ある男性を愛し、レズビアンの女性に愛された主人公の揺れる心の軌跡と伝播する愛の波のうねりが、繊細なタッチで語られる。公平(小日向文世)と不倫関係を続けている絹子(久野真紀子)は、結局は家族の元へと帰って行く公平を許しつつも、ひとりでいることの寂しさに胸を痛めている。ある日彼女は、真鍋(KEE)と聖(中村麻美)に出会う。聖は真鍋のところの居候で、恋人同士ではなく兄妹のように暮らしている。彼らの暖かさに、絹子は惹かれる。一方公平との付き合いは、すさんだものになっていく。そんなとき、聖が絹子に愛を告白する。レズビアンではない絹子は動揺するが、公平との関係に疲れ、聖の愛を求めるようになる。二人の生活が始まる。だがそこへ公平の娘が現れ、絹子のことが原因で公平が家を出て行ったのだと言う。再び絹子の動揺が始まる。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "143", "タイトル": "カタクリ家の幸福", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/82/e9/7a/df/24d1a2317744bc1fd0693610818925f6_640.jpg", "caption": "(c) 「カタクリ家の幸福」三池崇史監督 2001年 写真提供松竹株式会社"}, {"url": "//jfdb.jp/data/photo/movie/82/e9/7a/df/ce839a9bdb400092c671c0c40901cd9c_640.jpg", "caption": "(c) 「カタクリ家の幸福」三池崇史監督 2001年 写真提供松竹株式会社"}], "公開年月日": "2002年2月23日", "上映時間": "113 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "三池崇史", "name_id": "123", "職種": "監督"}, {"名前": "佐生哲雄", "id": "1340", "職種": "プロデューサー", "役割": ""}, {"名前": "吉田浩二", "id": "1341", "職種": "プロデューサー", "役割": ""}, {"名前": "山岸きくみ", "id": "1342", "職種": "スタッフ", "役割": "脚本"}, {"名前": "韓国映画「クワイエット・ファミリー」", "id": "1343", "職種": "スタッフ", "役割": "原作"}, {"名前": "野村明生", "id": "1344", "職種": "スタッフ", "役割": "撮影"}, {"名前": "馬飼野康ニ", "id": "1345", "職種": "スタッフ", "役割": "音楽"}, {"名前": "遠藤浩二", "id": "126", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "沢田研ニ", "id": "1346", "役割": "カタクリマサオ"}, {"名前": "松坂慶子", "id": "520", "役割": "カタクリテルエ"}, {"名前": "武田真治", "id": "1347", "役割": "カタクリマサユキ"}], "製作会社": ["「カタクリ家の幸福」製作委員会"], "制作会社": [], "配給会社（国内）": [], "説明": "韓国で大ヒットした『クワイエットファミリー』を、三池崇史がミュージカル仕立てのホラー・コメディに作り上げた。会社をリストラされたカタクリマサオ(沢田研二)は、人里離れた山中にペンションを建てる。マサオの夢が現実となったのだが、客は来ない。ようやくやってきた初めての客は、翌朝死体で発見される。警察に連絡したら、ペンションの名前に傷がつく。彼らは死体を裏山に埋める。そして数日後、2番目の客がやってくる。しかしまた翌朝、部屋には死体が。一家は再び死体を担いで裏山へと向かう。一方、娘のシズエ(西田尚美)は町で、アメリカ軍人リチャード佐川(忌野清志郎)と出会う。すっかり心を奪われたシズエだが、佐川は結婚詐欺師であった。それを見破った祖父ニヘイ(丹波哲郎)は格闘の末佐川を殺してしまう。そんな呪われたペンションに、朗報が舞い込む。ペンションの前に道路が開通するのだ。しかし、道路の通る場所は彼らが死体を生めた場所でもあった……。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "142", "タイトル": "凶気の桜", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/6a/b1/ff/da/76cc4126a6a1a29cc6dfbb414428d7ec_640.jpg", "caption": "(c) 2002 TOEI COMPANY, LTD."}, {"url": "//jfdb.jp/data/photo/movie/6a/b1/ff/da/da1321fb8fbbd5db9b6ad7fece06fa5d_640.jpg", "caption": "(c) 2002 TOEI COMPANY, LTD."}], "公開年月日": "2002年10月19日", "上映時間": "122 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "薗田賢次", "name_id": "1333", "職種": "監督"}, {"名前": "國松達也", "id": "1330", "職種": "プロデューサー", "役割": ""}, {"名前": "福吉健", "id": "1331", "職種": "プロデューサー", "役割": ""}, {"名前": "服部紹男", "id": "1332", "職種": "プロデューサー", "役割": ""}, {"名前": "丸山昇一", "id": "5315", "職種": "スタッフ", "役割": "脚本"}, {"名前": "ヒキタクニオ", "id": "1334", "職種": "スタッフ", "役割": "原作"}, {"名前": "仙元誠三", "id": "1335", "職種": "スタッフ", "役割": "撮影"}, {"名前": "K DUB SHINE", "id": "1336", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "窪塚洋介", "id": "177", "役割": "山口進"}, {"名前": "原田芳雄", "id": "350", "役割": "青田修三"}, {"名前": "江口洋介", "id": "2840", "役割": "Sweeper Saburo"}], "製作会社": ["「凶気の桜」製作委員会"], "制作会社": [], "配給会社（国内）": [], "説明": "ミュージックビデオ、音楽番組のディレクターとしてキャリアを積んだ薗田賢次の監督デビュー作。ストリートに生きる若者たちの純粋な怒りと鮮烈な生が、彼らを包む街の生々しい風景や最先端のヒップホップ・ミュージックとともに描かれる。山口(窪塚洋介)、市川(RIKIYA)、小菅(須藤元気)の3人は、白い戦闘服を身にまとい、彼らが集う渋谷の街から半端な不良たちを「掃除」していく。暴力団・青修同盟の会長の青田(原田芳雄)は3人を面白がり、特別に可愛がる。彼らも青田を慕い、次第に影響を受けるなかで、それぞれの道を見つめ始める。山口は、乱闘の最中に女子高生・景子(高橋マリ子)と運命的な出会いをする。そんなとき、青修同盟と小西組との抗争が勃発。ドラッグを捌いているというクラブを3人が襲ったのが、きっかけだった。その抗争と陰謀に3人も巻き込まれ、山口は市川と小菅を失う。そして山口も、景子と二人でいるところを撃たれてしまうのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "141", "タイトル": "クロエ", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/ef/fd/7e/59/70b35e53fa6dbe43e844ca27d393cf47_640.jpg", "caption": "(c) J WORKS FILM INITIATIVE"}, {"url": "//jfdb.jp/data/photo/movie/ef/fd/7e/59/fe0b18e2d56d6d3dc8216422b8a072f3_640.jpg", "caption": "(c) J WORKS FILM INITIATIVE"}], "公開年月日": "2002年6月15日", "上映時間": "128 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "利重剛", "name_id": "1325", "職種": "監督"}, {"名前": "仙頭武則", "id": "420", "職種": "プロデューサー", "役割": ""}, {"名前": "利重剛", "id": "1325", "職種": "スタッフ", "役割": "脚本"}, {"名前": "萩生田宏治", "id": "1326", "職種": "スタッフ", "役割": "脚本"}, {"名前": "ボリス・ヴィアン", "id": "1327", "職種": "スタッフ", "役割": "原作"}, {"名前": "篠田昇", "id": "1044", "職種": "スタッフ", "役割": "撮影"}, {"名前": "今野登茂子", "id": "1328", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "永瀬正敏", "id": "1088", "役割": "高太郎"}, {"名前": "ともさかりえ", "id": "1137", "役割": "クロエ"}, {"名前": "塚本晋也", "id": "54", "役割": "英助"}], "製作会社": ["WOWOW", "電通", "IMAGICA", "東京テアトル"], "制作会社": [], "配給会社（国内）": [], "説明": "ボリス・ヴィアンの小説『日々の泡』を原作とする、利重剛6年ぶりの新作。プラネタリウムで働く高太郎(永瀬正敏)はある時、黒枝(ともさかりえ)という女性と運命的な恋に落ちる。親友の英助(塚本晋也)、その彼女・日出美(松田美由紀)らに祝福される中、二人はささやかな結婚式を挙げ、幸せな日々が始まる。だが突然、黒枝は肺の中に睡蓮の花が咲くという原因不明の病に倒れる。その睡蓮の花の成長を抑えるにはなるべくたくさんの他の花が必要だということがわかり、花を買うためにどんなひどい仕事でもする高太郎。黒枝と共に過ごす時間も短くなっていく。英助は人気の絵本作家にのめりこみ散財して、高太郎らの前から姿を消す。日出美は英助を取り戻そうと絵本作家のもとを訪れ、彼を刺し殺す。それを知った高太郎は家へ急いで帰り、残された時間を黒枝と二人で過ごそうとする。ゆっくりと睡蓮が花開き、周りの人々の不幸と一緒に黒枝はこの世を去る。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "140", "タイトル": "刑務所の中", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/d4/d7/83/b7/7f402dbc52e663662674627e0b9c5022_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/d4/d7/83/b7/c0b287906963bf94bf76e619e4c8c392_640.jpg", "caption": ""}], "公開年月日": "2002年12月7日", "上映時間": "99 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "崔洋一", "name_id": "1319", "職種": "監督"}, {"名前": "榎望", "id": "159", "職種": "プロデューサー", "役割": ""}, {"名前": "崔洋一", "id": "1319", "職種": "スタッフ", "役割": "脚本"}, {"名前": "鄭義信", "id": "1320", "職種": "スタッフ", "役割": "脚本"}, {"名前": "中村義洋", "id": "1321", "職種": "スタッフ", "役割": "脚本"}, {"名前": "花輪和一", "id": "1322", "職種": "スタッフ", "役割": "原作"}, {"名前": "浜田毅", "id": "163", "職種": "スタッフ", "役割": "撮影"}, {"名前": "佐々木次彦", "id": "1323", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "山﨑努", "id": "1146", "役割": "ハナワ"}, {"名前": "香川照之", "id": "4797", "役割": "伊笠"}, {"名前": "田口トモロヲ", "id": "861", "役割": "田辺"}], "製作会社": ["ビーワイルド", "衛星劇場"], "制作会社": [], "配給会社（国内）": [], "説明": "ガンマニアのハナワ(山崎努)は拳銃不法所持などの罪で懲役3年の刑を受け、刑務所の雑居房に送られる。そこには、個性豊かな受刑者に囲まれた、軍隊以上に規則の厳しい生活が待っていた。日々の話題と言えば、掃除で出た陰毛の犯人探し、他の受刑者の噂話など、些細なことばかり。そして数少ない楽しみは、たまに出る甘いお菓子や免業日の映画観賞など。同じ日常の中にも不思議な規則や出来事があふれた刑務所で、ハナワたち雑居房の仲間は連帯感を強めていく。彼らはお互いの連絡先を教えあうが、そのメモが抜き打ち検査で見つかってしまい、ハナワたちは懲罰房に送られる。独房での生活は、ただ机に向かって黙々と薬袋を作るだけ。しかも一番風呂に入れるし、新聞も読める。そんな独房での生活にハナワは充実感すら感じてしまうのだった。自身の刑務所体験を描いた花輪和一の同名漫画を『月はどっちに出ている』の崔洋一が映画化。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "139", "タイトル": "ごめん", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/0b/7c/f9/9e/02d9a202d6c8da2f31e3fa44f92581eb_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/0b/7c/f9/9e/b3a7535aa90a0f4878db09890fc1e1d5_640.jpg", "caption": ""}], "公開年月日": "2002年10月12日", "上映時間": "103 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "冨樫森", "name_id": "1314", "職種": "監督"}, {"名前": "藤門浩之", "id": "1312", "職種": "プロデューサー", "役割": ""}, {"名前": "河野聡", "id": "1313", "職種": "プロデューサー", "役割": ""}, {"名前": "久保田傑", "id": "189", "職種": "プロデューサー", "役割": ""}, {"名前": "山田耕大", "id": "1213", "職種": "スタッフ", "役割": "脚本"}, {"名前": "ひこ・田中", "id": "1315", "職種": "スタッフ", "役割": "原作"}, {"名前": "上野彰吾", "id": "738", "職種": "スタッフ", "役割": "撮影"}, {"名前": "大友良英", "id": "245", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "久野雅弘", "id": "1316", "役割": "セイ"}, {"名前": "櫻谷由貴花", "id": "1317", "役割": "ナオコ"}, {"名前": "國村隼", "id": "1062", "役割": "セイの父"}], "製作会社": ["読売テレビ", "バンダイビジュアル", "舞夢プロ", "イエス・ビジョンズ", "オフィス・シロウズ"], "制作会社": [], "配給会社（国内）": [], "説明": "『台風クラブ』『光る女』などの相米慎二の作品や井筒和幸、平山秀幸らの助監督を務めた冨樫森の、『非・バランス』に続く長編劇映画第二作。セイこと七尾聖市(久野雅弘)は、大阪郊外に住む小学校6年生。ある日の授業中に突然射精してしまったセイは、クラスの中で一番早く自分のもとに訪れた身体の変化に戸惑ってしまう。そんな時にセイは京都の祖父母の家に行った際に見かけた女の子(桜谷由貴香)に恋をしてしまう。ナオコという名のその子の元を訪れたセイは、彼女が中学2年の「年上の女」である衝撃の事実を知る。なんとかセイは初デートにこぎつけるものの、離婚しているナオコの両親の話をしてしまい彼女の機嫌を損ねる。付き合ってほしいという一世一代のセイの告白も、ナオコはあっさり断る。人生最大の挫折を味わうセイ。しかしそれでもあきらめないセイは様々な困難を克服し、最後にはナオコはセイの頬にキスをしてくれるのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "138", "タイトル": "さゞなみ", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/2b/b2/51/30/bfbe2da7f238265a40cdb6ebc375f64d_640.jpg", "caption": "(c) 2002/「さざなみ」製作委員会 日活・バップ・博報堂・ハマーズ・NHKソフトウェア"}, {"url": "//jfdb.jp/data/photo/movie/2b/b2/51/30/e222358aaef6f005617243f0cd6848c0_640.jpg", "caption": "(c) 2002/「さざなみ」製作委員会 日活・バップ・博報堂・ハマーズ・NHKソフトウェア"}], "公開年月日": "2002年11月23日", "上映時間": "112 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "長尾直樹", "name_id": "1305", "職種": "監督"}, {"名前": "井上弘道", "id": "1299", "職種": "プロデューサー", "役割": ""}, {"名前": "安村重幸", "id": "1300", "職種": "プロデューサー", "役割": ""}, {"名前": "岡本東郎", "id": "1301", "職種": "プロデューサー", "役割": ""}, {"名前": "山崎康史", "id": "1302", "職種": "プロデューサー", "役割": ""}, {"名前": "古賀宗岳", "id": "1303", "職種": "プロデューサー", "役割": ""}, {"名前": "黒木敬士", "id": "1304", "職種": "プロデューサー", "役割": ""}, {"名前": "長尾直樹", "id": "1305", "職種": "スタッフ", "役割": "脚本"}, {"名前": "藤井保", "id": "1306", "職種": "スタッフ", "役割": "撮影"}, {"名前": "松島孝助", "id": "1564", "職種": "スタッフ", "役割": "撮影"}, {"名前": "岸谷香", "id": "1308", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "唯野未歩子", "id": "1309", "役割": "夏井稲子"}, {"名前": "松坂慶子", "id": "520", "役割": "夏井澄江"}, {"名前": "豊川悦司", "id": "5620", "役割": "玉水龍男"}], "製作会社": ["日活", "バップ", "博報堂", "ハマーズ", "NHKソフトウェア"], "制作会社": [], "配給会社（国内）": [], "説明": "CMやミュージックビデオなどの分野でも活躍する長尾直樹の、『鉄塔武蔵野線』に続く劇場映画第三作。水質調査員として市役所に勤める夏井稲子(唯野未歩子)は調査に訪れた温泉旅館で便利屋の玉水(豊川悦司)に出会う。何かと悪い噂のたえない玉水だが、ある出来事をきっかけに、稲子は心惹かれるようになる。一方、今はひとり離れて暮らす稲子の母・澄江の元に稲子がまだ幼い頃にブラジルで死んだはずの夫から手紙が届いた。澄江はただ黙って遠くを見つめる。ある日玉水のアパートを訪ねた稲子を迎えたのは玉水本人ではなく、息子だった。動揺をかくせない稲子に玉水は近々街を去ることが知らされる。実家に帰省中、澄江と家族の思い出の場所である温泉に行った稲子。彼女は思いがけず母の涙を見てしまい、長い年月が経ってなお母の父への想いの深さを知る。そのことが稲子に自分の玉水への想いを確かなものにさせたのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "137", "タイトル": "助太刀屋助六", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/bf/a9/91/2f/06e5b8aafc9da79a857efe8db2a202a4_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/bf/a9/91/2f/74d6b345eb1a8c17507ed0b745a0af0a_640.jpg", "caption": ""}], "公開年月日": "2002年2月16日", "上映時間": "88 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "岡本喜八", "name_id": "1294", "職種": "監督"}, {"名前": "石丸省一郎", "id": "1291", "職種": "プロデューサー", "役割": ""}, {"名前": "西村維樹", "id": "1292", "職種": "プロデューサー", "役割": ""}, {"名前": "藤倉博", "id": "1293", "職種": "プロデューサー", "役割": ""}, {"名前": "岡本喜八", "id": "1294", "職種": "スタッフ", "役割": "脚本"}, {"名前": "生田大作", "id": "1295", "職種": "スタッフ", "役割": "原作"}, {"名前": "加藤雄大 (J.S.C)", "id": "1020", "職種": "スタッフ", "役割": "撮影"}, {"名前": "山下洋輔", "id": "1296", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "真田広之", "id": "1264", "役割": "助太刀屋助六"}, {"名前": "鈴木京香", "id": "1176", "役割": "お仙"}, {"名前": "村田雄浩", "id": "1297", "役割": "太郎"}], "製作会社": ["日活", "フジテレビジョン"], "制作会社": [], "配給会社（国内）": [], "説明": "長年日本の映画界をリードし続けてきた鬼才・岡本喜八が贈る本格痛快時代劇。助六(真田広之)は仇討ちをする人の手助け、助太刀を生業とする風来坊。7年ぶりに帰ってきた故郷で、偶然にも正に仇討ちが行われようとする現場に居合わせる。が、敵とされる老侍(仲代達也)と会った助六は、彼は敵の面をしていないと感じとる。助六は老侍に共感さえ覚えるのだが、そのとき突然彼にアテ身をくらわされ、助六は気を失ってしまう。「本当のことを言うな」と、そばにいた訳知りの男に言い残し、仇討ちが待つ往来へ出て行く老侍。銃声で目を覚ました助六が往来へ飛び出すそうとすると、そこには絶命した老侍の姿があった。そして、その埋葬の過程で、彼はその老侍が自分の実の父・片倉梅太郎であったという事実や、父が仇討ちされる目にあったことはこの街を取り仕切る役人(岸部一徳)の罠であったことを知る。仇討ちの本当の意味を知った助六は、自分の父を殺した敵に闘いを挑み、仇討ちを果たすのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "136", "タイトル": "宣戦布告", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/3b/09/5e/68/ec5dbdfd50301898e1683805214673c3_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/3b/09/5e/68/3ee4c02330597e30169139093fa0ddc2_640.jpg", "caption": ""}], "公開年月日": "2002年10月5日", "上映時間": "106 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "石侍露堂", "name_id": "1284", "職種": "監督"}, {"名前": "石侍露堂", "id": "1284", "職種": "プロデューサー", "役割": ""}, {"名前": "小松與志子", "id": "1285", "職種": "スタッフ", "役割": "脚本"}, {"名前": "石侍露堂", "id": "1284", "職種": "スタッフ", "役割": "脚本"}, {"名前": "麻生幾", "id": "1286", "職種": "スタッフ", "役割": "原作"}, {"名前": "阪本善尚", "id": "80", "職種": "スタッフ", "役割": "撮影"}, {"名前": "米村武", "id": "1287", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "古谷一行", "id": "1288", "役割": "諸橋揆一郎"}, {"名前": "夏八木勲", "id": "1289", "役割": "瀬川守良"}, {"名前": "杉本哲太", "id": "1290", "役割": "寺崎秀一"}], "製作会社": ["ウィル"], "制作会社": [], "配給会社（国内）": [], "説明": "日本海沿岸で、一隻の国籍不明の潜水艦が座礁した。浮上した潜水艦内からは、戦闘服に身を包んだ男たちが出現し、夜闇の中へ姿を消した。潜水艦の乗組員のひとりが捕まり、北東人民共和国の工作員たちが侵入したことが判明。一旦特殊部隊に射殺命令が出されたが、政治的な問題を考え命令を解除したその瞬間、相手方のロケット砲が発射され、隊員たちは死亡。警察では手に負えない今、首相官邸では自衛隊を出動させるかどうか議論は紛糾していた。そんな中民間人にも犠牲者が出てしまい、諸橋首相(古谷一行)は、先制攻撃はしない姿勢ながらも、自衛隊の治安出動命令を発することを決意。そこに、北東人民共和国が核ミサイル発射体制に入ったという映像が入り、アメリカを始めとする各国も軍事を動かし始めた。もう戦争は免れないと思われたその時、共和国が撤退、日本は最大の危機を免れた。日本の安全保障の問題を鋭く描いたベストセラー小説の映画化。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "135", "タイトル": "千年女優", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/62/69/b0/fb/3551882ae9c7911d78d10c72c254378a_640.jpg", "caption": "(c) 「千年女優」製作委員会"}, {"url": "//jfdb.jp/data/photo/movie/62/69/b0/fb/ee889ddfa6fea8513d9711222eee4d47_640.jpg", "caption": "(c) 「千年女優」製作委員会"}], "公開年月日": "2002年9月14日", "上映時間": "87 分", "カテゴリー": ["アニメーション"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "今敏", "name_id": "394", "職種": "監督"}, {"名前": "真木太郎", "id": "393", "職種": "プロデューサー", "役割": ""}, {"名前": "丸山正雄", "id": "333", "職種": "プロデューサー", "役割": ""}, {"名前": "今敏", "id": "394", "職種": "スタッフ", "役割": "脚本"}, {"名前": "村井さだゆき", "id": "1278", "職種": "スタッフ", "役割": "脚本"}, {"名前": "白井久男", "id": "336", "職種": "スタッフ", "役割": "撮影"}, {"名前": "平沢進", "id": "1279", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "小山茉美", "id": "1280", "役割": "藤原千代子"}, {"名前": "飯塚昭三", "id": "1281", "役割": "立花源也"}, {"名前": "小野坂昌也", "id": "1282", "役割": "井田恭ニ"}], "製作会社": ["クロックワークス", "ジェンコ", "WOWOW", "バンダイビジュアル", "角川書店"], "制作会社": [], "配給会社（国内）": [], "説明": "『A.I.』や『シュレック』のドリームワークス社が史上始めて日本映画の配給権を獲得した1本。監督は『パーフェクトブルー』の今敏。かつては大手であった映画会社の撮影所が取り壊されることになった。そこでテレビの制作会社の立花はこの会社の黄金時代を支えた女優・藤原千代子のドキュメンタリー番組を企画する。人里離れた山奥に住んでいる千代子を訪ねる立花。彼女は数奇な人生を語り出す。映画女優としてスカウトされた千代子は返事を渋っていた。帰り道で警官に追われる絵描きを助けた千代子は彼に淡い想いを抱く。翌日にはもう彼の姿は見えず、彼を追って満州へ向かうため千代子は女優になる決意をする。現実と映画の中の虚構が入り交じり、未来と過去を行き来して千代子は運命の男を追いかけつづける。話し終えた後で急に倒れた千代子は病院へと運ばれる。死の近いことを悟る千代子は、それでも好きな人を追い続ける自分の姿が好きなのだ、と微笑む。", "公式サイト": [], "映画祭・受賞歴": ["2018 アヌシー国際アニメーション映画祭 Special Programmes"]}
{"id": "134", "タイトル": "ソウル", "予告動画": "", "image": [], "公開年月日": "2002年2月9日", "上映時間": "109 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "長澤雅彦", "name_id": "1141", "職種": "監督"}, {"名前": "亀山千広", "id": "1269", "職種": "プロデューサー", "役割": ""}, {"名前": "島谷能成", "id": "1270", "職種": "プロデューサー", "役割": ""}, {"名前": "気賀純夫", "id": "1271", "職種": "プロデューサー", "役割": ""}, {"名前": "藤島ジュリーK.", "id": "5840", "職種": "プロデューサー", "役割": ""}, {"名前": "長谷川康夫", "id": "1273", "職種": "スタッフ", "役割": "脚本"}, {"名前": "山本英夫 (J.S.C.)", "id": "81", "職種": "スタッフ", "役割": "撮影"}, {"名前": "住友紀人", "id": "1274", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "長瀬智也", "id": "1276", "役割": "早瀬祐太郎"}, {"名前": "チェ・ミンス", "id": "1277", "役割": "キム・ユンチョル"}], "製作会社": ["フジテレビジョン", "東宝", "電通", "ジャニーズ出版", "ポニーキャニオン", "デスティニー", "エンジンネットワーク"], "制作会社": [], "配給会社（国内）": [], "説明": "岩井俊二の『Undo』『LoveLetter』『PICNIC』等のプロデューサーを務め、『ココニイルコト』で監督デビューを果たした長澤雅彦の最新作。逃亡犯を護送して韓国に来ていた新米刑事・早瀬祐太郎(長瀬智也)は、成田行きの飛行機に乗り遅れそうになる。慌ててソウルの街を走っていたところ、彼は現金輸送車強盗事件に巻き込まれてしまう。そのドサクサの中で祐太郎は犯人を追い詰めるのだが、一人を逃がすことになる。犯人を唯一目撃している彼に、72時間の滞在許可が出され、犯人の顔を特定する作業が命じられる。この事件の担当であるソウル市警刑事部長のキム・ユンチョル(チェ・ミンス)とは、言葉の壁、慣習の差があり、理解し合えない。そんな中、この事件は大規模な犯罪組織もからんだものだとわかってくる。捜査の撹乱の裏にある犯人の本当の狙いに気付き、彼らはたった二人で真犯人を追跡する。その追跡の過程で二人は理解しあい、無事に事件を解決する。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "133", "タイトル": "たそがれ清兵衛", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/77/76/9b/63/ae96bcdbe7de53bed0fada0288313161_640.jpg", "caption": "(c) 「たそがれ清兵衛」山田洋次監督 2002年 写真提供松竹株式会社"}, {"url": "//jfdb.jp/data/photo/movie/77/76/9b/63/88e10b441ae937be7cc154ddb5ed7721_640.jpg", "caption": "(c) 「たそがれ清兵衛」山田洋次監督 2002年 写真提供松竹株式会社"}], "公開年月日": "2002年11月2日", "上映時間": "129 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "山田洋次", "name_id": "405", "職種": "監督"}, {"名前": "中川滋弘", "id": "1259", "職種": "プロデューサー", "役割": ""}, {"名前": "深澤宏", "id": "403", "職種": "プロデューサー", "役割": ""}, {"名前": "山本一郎", "id": "1260", "職種": "プロデューサー", "役割": ""}, {"名前": "山田洋次", "id": "405", "職種": "スタッフ", "役割": "脚本"}, {"名前": "朝間義隆", "id": "1814", "職種": "スタッフ", "役割": "脚本"}, {"名前": "藤沢周平", "id": "1261", "職種": "スタッフ", "役割": "原作"}, {"名前": "長沼六男", "id": "1262", "職種": "スタッフ", "役割": "撮影"}, {"名前": "冨田勲", "id": "1263", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "真田広之", "id": "1264", "役割": "井口清兵衛"}, {"名前": "宮沢りえ", "id": "1265", "役割": "飯沼朋江"}, {"名前": "丹波哲郎", "id": "1236", "役割": "井口藤左衛門"}], "製作会社": ["松竹", "日本テレビ", "住友商事", "博報堂", "日販", "衛星劇場"], "制作会社": [], "配給会社（国内）": [], "説明": "『男はつらいよ』『幸福の黄色いハンカチ』『学校』等の作品を生みだしてきた日本映画界の巨匠・山田洋次がおくる初の本格時代劇。下級武士の井口清兵衛(真田広之)は妻に先立たれ、幼い二人の子と痴呆気味の老いた母親をわずかな給料でなんとか養っている。そのため彼は夕暮れ時になり勤めが終わると、上司や同僚の誘いを断ってまっすぐ家に帰り内職をする。そんな彼を同僚たちは陰で「たそがれ清兵衛」と綽名して呼んでいた。ある日、彼はかつて想いをよせていた幼なじみ・朋江(宮沢りえ)を別れた夫の暴力から救う。その時の剣の腕を見込まれて、清兵衛はある男を討つようにと藩命を受ける。決行の当日、清兵衛は朋江に秘めていた想いを打ち明け、出発する。屋敷の中に籠もった男は凄腕の剣術使いであり、清兵衛は苦戦する。それでも何とか男を倒した清兵衛は、二人の娘と老母、そして朋江の待つ我が家へと帰るのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "132", "タイトル": "黄昏流星群/星のレストラン", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/9c/06/bc/59/02f0211fbd0598e38e43ac2591bac8c5_640.jpg", "caption": "(c) 2001 弘兼憲史シネマ劇場製作委員会"}, {"url": "//jfdb.jp/data/photo/movie/9c/06/bc/59/9099528ff1712d719f8cde8a917a57e7_640.jpg", "caption": "(c) 2001 弘兼憲史シネマ劇場製作委員会"}], "公開年月日": "2002年5月18日", "上映時間": "99 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "富岡忠文", "name_id": "1253", "職種": "監督"}, {"名前": "八木廣", "id": "1251", "職種": "プロデューサー", "役割": ""}, {"名前": "岡本晃一", "id": "1252", "職種": "プロデューサー", "役割": ""}, {"名前": "富岡忠文", "id": "1253", "職種": "スタッフ", "役割": "脚本"}, {"名前": "弘兼憲史", "id": "1254", "職種": "スタッフ", "役割": "原作"}, {"名前": "佐々木原保志 (JSC)", "id": "1080", "職種": "スタッフ", "役割": "撮影"}, {"名前": "百石元", "id": "1255", "職種": "スタッフ", "役割": "音楽"}, {"名前": "西村ヒロ", "id": "1256", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "石橋蓮司", "id": "23", "役割": "立松一平"}, {"名前": "鳥羽潤", "id": "604", "役割": "片岡ヒロミ"}, {"名前": "板谷由夏", "id": "1257", "役割": "今井美奈"}], "製作会社": ["弘兼憲史シネマ劇場製作委員会", "電通", "日本ビクター", "BS-i"], "制作会社": [], "配給会社（国内）": [], "説明": "社会派漫画家、弘兼憲史の原作・監修による大人の恋愛ドラマ。高級フランス料理店で働くヒロミ(鳥羽潤)はある日ひとりの初老の男(石橋蓮司)と知り合う。ただの老人にしか見えない男は鋭い味覚の持ち主であり、ヒロミの料理の問題点を的確に言い当て、料理の腕前もただものではない。彼の正体はかつてフランスで活躍した伝説の料理人・立松一平であった。一平はフランスで食中毒事件を起こしたために、婚約者だったソフィー(AnneLefol)の元を去り、それ以来料理を捨てたという過去をもっていた。ヒロミは一平の料理に心酔し、彼に料理を教えてもらう。そんな中、今では世界的な食器メーカーの社長夫人となったソフィーが来日することに。来日の日、ヒロミの働く店を借り一平は自分の料理を彼女に食べさせる。その味で料理を作ったのがかつての恋人・一平だと気付くソフィー。二人は束の間の再会をはたし、それぞれの生活に戻っていった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "131", "タイトル": "チキン・ハート", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/eb/99/79/41/43789afd33e309fb52ea58214de61626_640.jpg", "caption": "(c) 2002 Bandai Visual, Tokyo FM, Television Tokyo and Office Kitano"}, {"url": "//jfdb.jp/data/photo/movie/eb/99/79/41/f7b29922f7685e588448ed3b8e9502a4_640.jpg", "caption": "(c) 2002 Bandai Visual, Tokyo FM, Television Tokyo and Office Kitano"}], "公開年月日": "2002年8月3日", "上映時間": "105 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "清水浩", "name_id": "1246", "職種": "監督"}, {"名前": "森昌行", "id": "5010", "職種": "プロデューサー", "役割": ""}, {"名前": "吉田多喜男", "id": "1245", "職種": "プロデューサー", "役割": ""}, {"名前": "清水浩", "id": "1246", "職種": "スタッフ", "役割": "脚本"}, {"名前": "高瀬比呂志", "id": "226", "職種": "スタッフ", "役割": "撮影"}, {"名前": "鈴木慶一", "id": "397", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "池内博之", "id": "1160", "役割": "岩野修"}, {"名前": "忌野清志郎", "id": "1247", "役割": "浅田智史"}, {"名前": "松尾スズキ", "id": "1248", "役割": "丸誠ニ"}], "製作会社": ["バンダイビジュアル", "エフエム東京", "テレビ東京", "オフィス北野"], "制作会社": [], "配給会社（国内）": [], "説明": "元ボクサーの岩野(池内博之)は、同じアパートに住むサダさん(忌野清志郎)と丸さん(松尾スズキ)と組んで、盛り場の片隅で酔っぱらいのサラリーマンに殴られることで金を稼ぐ毎日を送っている。何もやりたいことがみつからない岩野を始め、3人は逃げられなくなる状況に自分を置くことを避け続けてきた。ある日、そんな3人に、アパート取り壊しの通知が舞い込んだ。立退き料は滞納した部屋代と棒引きにされ、居場所を失った3人はそれぞれの道を模索し始める。が、自分の夢だった自由気ままな旅に出ようとするサダさん、要領が悪いながらもまともな仕事に就いた丸さんを横に、岩野はいま一歩がどうしても踏み出せない。しかしちょっとした女性問題をきっかけとして、岩野は次第に自分の道を開いていくようになる。北野武の助監督などを経た後、デビュー作『生きない』でロカルノ映画祭アキュメニカル特別賞を受賞した、清水浩監督の第二作目。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "130", "タイトル": "突入せよ!『あさま山荘』事件", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/7a/3b/bf/35/23e06ac164427a6535f2764ab590f4df_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/7a/3b/bf/35/8e2f1367802d82d64cc3b236090acf44_640.jpg", "caption": ""}], "公開年月日": "2002年5月11日", "上映時間": "113 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "原田眞人", "name_id": "1239", "職種": "監督"}, {"名前": "原正人", "id": "1238", "職種": "プロデューサー", "役割": ""}, {"名前": "濱名一哉", "id": "107", "職種": "プロデューサー", "役割": ""}, {"名前": "鍋島壽夫", "id": "895", "職種": "プロデューサー", "役割": ""}, {"名前": "原田眞人", "id": "1239", "職種": "スタッフ", "役割": "脚本"}, {"名前": "佐々淳行", "id": "1240", "職種": "スタッフ", "役割": "原作"}, {"名前": "阪本善尚", "id": "80", "職種": "スタッフ", "役割": "撮影"}, {"名前": "村松崇継", "id": "1241", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "役所広司", "id": "385", "役割": "佐々淳行"}, {"名前": "宇崎竜童", "id": "1242", "役割": "宇田川信一"}, {"名前": "天海祐希", "id": "1243", "役割": "佐々幸子"}], "製作会社": ["東映", "アスミック・エース・エンタテインメント", "TBS", "産経新聞"], "制作会社": [], "配給会社（国内）": [], "説明": "1972年2月、軽井沢の「あさま山荘」に、連合赤軍5人が管理人の妻を人質に立て籠る事件が発生。警察庁の佐々淳行(役所広司)は長官(藤田まこと)から現場の指揮を任され、現場に急行する。数日で解決すると思われた事件は、銃器使用の許可が出ないことに加え、極限の寒さや警視庁と県警間の対立が続き、困難を極める。事件発生から10日目、クレーン車で操作する鉄球によって山荘の外壁を壊し、突破口を開くという作戦で、ついに突入が開始される。しかし途中クレーンの故障によって作戦の続行が難しくなり、前線は混乱に陥る。佐々は殉死した機動隊長に変わり、自ら最前線に立ち、突入を続行。彼の指揮に従って機動隊はついに犯人のいる部屋へ踏み込む。犯人たちはその場で全員取り押さえられ、人質も無事に保護された。日本中を揺るがした歴史的事件、「あさま山荘事件」を『金融腐食列島呪縛』の原田眞人が緻密な描写で描く。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "129", "タイトル": "猫の恩返し", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/58/dc/9c/c2/7aeb29a8b0b303d5504b0c97305b8a7a_640.jpg", "caption": "(c) 2002 猫乃手堂・Studio Ghibli・NDHMT"}, {"url": "//jfdb.jp/data/photo/movie/58/dc/9c/c2/3e60c4a98fc11f7635aeb8b4d2e03a7a_640.jpg", "caption": "(c) 2002 猫乃手堂・Studio Ghibli・NDHMT"}], "公開年月日": "2002年7月20日", "上映時間": "75 分", "カテゴリー": ["アニメーション"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "森田宏幸", "name_id": "1231", "職種": "監督"}, {"名前": "鈴木敏夫", "id": "1229", "職種": "プロデューサー", "役割": ""}, {"名前": "高橋望", "id": "1230", "職種": "プロデューサー", "役割": ""}, {"名前": "吉田玲子", "id": "1232", "職種": "スタッフ", "役割": "脚本"}, {"名前": "柊あおい", "id": "1233", "職種": "スタッフ", "役割": "原作"}, {"名前": "野見祐二", "id": "1234", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "池脇千鶴", "id": "504", "役割": "ハル"}, {"名前": "袴田吉彦", "id": "1235", "役割": "バロン"}, {"名前": "丹波哲郎", "id": "1236", "役割": "猫王"}], "製作会社": ["徳間書店", "スタジオジブリ", "日本テレビ", "ディズニー", "博報堂", "三菱商事"], "制作会社": [], "配給会社（国内）": [], "説明": "『もののけ姫』『千と千尋の神隠し』の宮崎駿の企画による、森田宏幸の劇場用初監督作品。女子高校生ハルはトラックに轢かれそうになった猫を助ける。その猫は人間のように立ち上がって礼を言い、去っていった。ハルが助けた猫は猫の国の王子・ルーンで、ハルは恩返しに猫の国に招待され、ルーンの父・猫王にルーンの妃となる約束をして帰ってきてしまう。困ったハルはどこからか聴こえてくる不思議な声に誘われて、猫の事務所に辿り着く。事務所の主人バロンはハルの話を聞き、猫の国に行って話をつけてくれると約束する。そこに猫王の側近が猫の大群とともに現れ、ハルをさらっていってしまう。婚礼の用意が進む猫の国の中で、夜明けまでにここを脱出しなければ自分も猫になってしまうことを知るハル。そこに駆け付けたバロンの活躍によって、ハルは無事人間の世界へと帰ってくることができた。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "128", "タイトル": "ハッシュ!", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/42/a1/39/2a/25edc2262909e10adf3050b85b5c8bc1_640.jpg", "caption": "(c) Siglo, Ltd. 2001"}, {"url": "//jfdb.jp/data/photo/movie/42/a1/39/2a/d4561f35732b8461868f2b4b14466fb7_640.jpg", "caption": "(c) Siglo, Ltd. 2001"}], "公開年月日": "2002年4月27日", "上映時間": "135 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "橋口亮輔", "name_id": "1220", "職種": "監督"}, {"名前": "山上徹二郎", "id": "4857", "職種": "プロデューサー", "役割": ""}, {"名前": "石川富康", "id": "347", "職種": "プロデューサー", "役割": ""}, {"名前": "塚田博男", "id": "1219", "職種": "プロデューサー", "役割": ""}, {"名前": "橋口亮輔", "id": "1220", "職種": "スタッフ", "役割": "原作"}, {"名前": "橋口亮輔", "id": "1220", "職種": "スタッフ", "役割": "原作"}, {"名前": "上野彰吾", "id": "738", "職種": "スタッフ", "役割": "撮影"}, {"名前": "ボビー・マクファーリン", "id": "1221", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "田辺誠一", "id": "79", "役割": "栗田勝裕"}, {"名前": "片岡礼子", "id": "5622", "役割": "藤倉朝子"}, {"名前": "高橋和也", "id": "1223", "役割": "長谷直也"}], "製作会社": ["シグロ", "衛星劇場", "テレシスインターナショナル"], "制作会社": [], "配給会社（国内）": [], "説明": "『二十才の微熱』『渚のシンドバッド』と同性愛をテーマにした作品を作ってきた橋口亮輔監督の、5年ぶりの長編。ゲイのカップル、直也(高橋和也)と勝裕(田辺誠一)は、偶然知り合った朝子(片岡礼子)から、「子作り」の相談を持ちかけられる。それまでは他人との深い関係を避け、無為なセックスを繰り返してきた朝子は、勝裕の精子を人工授精して赤ちゃんを産みたいと言う。彼らがゲイだと知った上でのこの提案は、直也と勝裕を動揺させる。そこへ彼らそれぞれの家族がやってきて、家族のあり方を説く。だがすでに旧式の家族関係から一歩踏み出してしまっている彼らにとって、それは何の助けにもならない。だがそんな家族との対立や軋轢の中で、彼らは次第に自分たちなりの新しい生き方を見つけていく。そしてある日、勝裕の兄が事故死する。古い家族のしきたりの中で生きてきた兄の死は、勝裕に、彼の踏み出した一歩の大きさとその意味を、深い悲しみと共に知らせるのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "127", "タイトル": "ピカレスク 人間失格", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/26/f6/02/b5/33928e16b8a7130b1c56d211375b0fc4_640.jpg", "caption": "(c) 2002 猪瀬直樹/グランプリ"}, {"url": "//jfdb.jp/data/photo/movie/26/f6/02/b5/6443a13daf91d90a3a43037b4a7245a6_640.jpg", "caption": "(c) 2002 猪瀬直樹/グランプリ"}], "公開年月日": "2002年7月27日", "上映時間": "133 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "伊藤秀裕", "name_id": "1212", "職種": "監督"}, {"名前": "小野誠一", "id": "1210", "職種": "プロデューサー", "役割": ""}, {"名前": "佐藤敏宏", "id": "1211", "職種": "プロデューサー", "役割": ""}, {"名前": "山田耕大", "id": "1213", "職種": "スタッフ", "役割": "脚本"}, {"名前": "猪瀬直樹", "id": "1214", "職種": "スタッフ", "役割": "原作"}, {"名前": "安藤庄平", "id": "1215", "職種": "スタッフ", "役割": "撮影"}, {"名前": "大島ミチル", "id": "668", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "河村隆一", "id": "1216", "役割": "太宰治"}, {"名前": "佐野史郎", "id": "1217", "役割": "井伏鱒二"}, {"名前": "とよた真帆", "id": "424", "役割": "山口富美栄"}], "製作会社": ["グランプリ"], "制作会社": [], "配給会社（国内）": [], "説明": "20世紀前半の日本文学を代表する作家のひとり・太宰治が幾度も自殺未遂、心中未遂を繰り返しながらも、生き続けた様に焦点を当て、その半生を描く。昭和23年6月19日、玉川上水に太宰治(河村隆一)と山口冨美栄(とよた真帆)の情死体が上がった。太宰治、享年39歳。しかし、太宰が心中を計ったのはそれが初めてではない。彼が最初に心中未遂事件を起こしたのは21歳の時。初枝(さとう珠緒)という同棲相手がいながら、深い仲でもないカフェの女給(朱門みず穂)と心中を計り、結果その女給だけが命を落した。その後も、太宰は執筆活動を続ける一方で、酒や薬に溺れ入退院や自殺未遂を繰り返す自堕落な日々を送る。井伏鱒二(佐野史郎)の媒酌で西原佐知子(裕木奈江)と結婚した頃から、作家としての評価も上がるが、相変わらず女性関係は複雑。そんな中で恋に落ちた未亡人の冨美栄は次第に太宰との心中を望むようになる。そしてついに二人は川に身を投げたのだった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "126", "タイトル": "陽はまた昇る", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/47/cb/97/69/cdc7dce2ec30be5fe2119a5a143a2093_640.jpg", "caption": "(c) 2002 TOEI COMPANY, LTD."}, {"url": "//jfdb.jp/data/photo/movie/47/cb/97/69/a364868b8871e4ca3473d4a4ccfcbdd8_640.jpg", "caption": "(c) 2002 TOEI COMPANY, LTD."}], "公開年月日": "2002年6月15日", "上映時間": "108 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "佐々部清", "name_id": "1204", "職種": "監督"}, {"名前": "厨子稔雄", "id": "480", "職種": "プロデューサー", "役割": ""}, {"名前": "小松茂明", "id": "1203", "職種": "プロデューサー", "役割": ""}, {"名前": "西岡琢也", "id": "1205", "職種": "スタッフ", "役割": "脚本"}, {"名前": "佐々部清", "id": "1204", "職種": "スタッフ", "役割": "脚本"}, {"名前": "佐藤正明", "id": "1206", "職種": "スタッフ", "役割": "原作"}, {"名前": "木村大作", "id": "1207", "職種": "スタッフ", "役割": "撮影"}, {"名前": "大島ミチル", "id": "668", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "西田敏行", "id": "411", "役割": "加賀谷静男"}, {"名前": "渡辺謙", "id": "488", "役割": "大久保修"}, {"名前": "仲代達矢", "id": "1208", "役割": "松下孝之助"}], "製作会社": ["「陽はまた昇る」製作委員会"], "制作会社": [], "配給会社（国内）": [], "説明": "家電メーカー日本ビクターに勤める加賀谷静男(西田敏行)は、20%の人員削減という社命を課せられ、横浜工場ビデオ事業部に異動になる。しかし加賀谷は人員削減を避け、家庭用VTRの開発によって業績を伸ばそうとする。「自分たちの職場は自分たちで守ろう」という加賀谷の熱い呼び掛けに胸を打たれた従業員たちは、商品開発に全力を尽くす。その努力のもと、VHSと名付けられた試作機が完成する。家庭用VTRを世に広めるには、互換性が最重要課題だと考えた加賀谷は試作機の情報を全面的に公開する。しかし通産省はSONYが先立って発売した家庭用VTRを国内統一規格とする調整に乗り出していた。加賀谷は家電業界の最大手・松下電器の相談役(仲代達也)に直訴するという最後の賭に出る。松下電器はVHSを絶賛し、採用を決定。ついにVHSは発売された。世界標準規格に上り詰めたVHSの誕生秘話。監督の佐々部清は本作でデビュー。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "125", "タイトル": "ピンポン", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/e4/c9/72/28/88da517d77292dd467abb0d7efbf69a5_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/e4/c9/72/28/de49ddf08fde67c195f40b7809225e79_640.jpg", "caption": ""}], "公開年月日": "2002年7月20日", "上映時間": "114 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "曽利文彦", "name_id": "1196", "職種": "監督"}, {"名前": "小川真司", "id": "498", "職種": "プロデューサー", "役割": ""}, {"名前": "鈴木早苗", "id": "1194", "職種": "プロデューサー", "役割": ""}, {"名前": "井上文彦", "id": "1195", "職種": "プロデューサー", "役割": ""}, {"名前": "宮藤官九郎", "id": "285", "職種": "スタッフ", "役割": "脚本"}, {"名前": "松本大洋", "id": "1197", "職種": "スタッフ", "役割": "原作"}, {"名前": "佐光朗", "id": "1198", "職種": "スタッフ", "役割": "撮影"}, {"名前": "二見裕志", "id": "1199", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "窪塚洋介", "id": "177", "役割": "ぺコ"}, {"名前": "ARATA", "id": "1200", "役割": "スマイル"}, {"名前": "竹中直人", "id": "1201", "役割": "コーチ"}], "製作会社": ["アスミック・エース・エンタテインメント", "小学館", "TBS", "BS-i", "IMAGICA", "日販"], "制作会社": [], "配給会社（国内）": [], "説明": "若者に絶大な人気を誇る松本大洋の卓球漫画の世界がCGを駆使して描かれる。ペコ(窪塚洋介)とスマイル(ARATA)は卓球部に所属する幼馴染み。卓球で世界一になることを目指すペコに対して、卓球はただの暇つぶしだと言うスマイル。だが、コーチ(竹中直人)が眼を掛けたのは、ペコではなくスマイルだった。大会で自分よりも弱かったはずの相手に完敗したペコは卓球から逃げ、それを機にスマイルは心を閉ざし、ひたすら練習に没頭するようになる。しかし、あるきっかけからペコは再び卓球を再開、猛特訓を始める。再び大会で顔を合わせた二人。準決勝まで勝ち上がったペコだったが、負傷した足はもう限界の状態。棄権を勧める周囲をよそに、「ずっとスマイルが俺のことを待っているから」と試合を続けるペコ。昨年の優勝者・風間(中村獅童)を見事に敗り、ペコはスマイルの待つ決勝戦に進む。その時スマイルは呟いた、「おかえり、ヒーロー」と。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "124", "タイトル": "ミスター・ルーキー", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/3e/44/0d/06/57d658d3b6419bcc5194ea1e908970e0_640.jpg", "caption": "(c) 2002 『ミスター・ルーキー』製作委員会"}, {"url": "//jfdb.jp/data/photo/movie/3e/44/0d/06/5b214e69b4b8c2be513951f2f6f90c18_640.jpg", "caption": "(c) 2002 『ミスター・ルーキー』製作委員会"}], "公開年月日": "2002年3月23日", "上映時間": "118 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "井坂聡", "name_id": "5248", "職種": "監督"}, {"名前": "田村三勇", "id": "1184", "職種": "プロデューサー", "役割": ""}, {"名前": "赤井淳司", "id": "1185", "職種": "プロデューサー", "役割": ""}, {"名前": "清水啓太郎", "id": "1186", "職種": "プロデューサー", "役割": ""}, {"名前": "伊地智啓", "id": "1183", "職種": "スタッフ", "役割": "エグゼクティヴ・プロデューサー"}, {"名前": "井坂聡", "id": "5248", "職種": "スタッフ", "役割": "脚本"}, {"名前": "鈴木崇", "id": "1187", "職種": "スタッフ", "役割": "脚本"}, {"名前": "佐藤佐吉", "id": "828", "職種": "スタッフ", "役割": "原案"}, {"名前": "佐野鉄郎", "id": "1188", "職種": "スタッフ", "役割": "撮影"}, {"名前": "和田薫", "id": "5042", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "長嶋一茂", "id": "1189", "役割": "大原幸嗣"}, {"名前": "鶴田真由", "id": "1190", "役割": "大原優子"}, {"名前": "橋爪功", "id": "1191", "役割": "瀬川監督"}], "製作会社": ["「ミスター・ルーキー」製作委員会", "全国朝日放送", "IMAGICA", "衛星劇場", "江崎グリコ", "角川書店", "ソニー デサント", "電通関西"], "制作会社": [], "配給会社（国内）": [], "説明": "200X年、突如現れた覆面投手「ミスター・ルーキー」の活躍により、プロ野球の阪神タイガースは快進撃を続けていた。実はその正体は普通のサラリーマン、大原幸嗣(長嶋一茂)。彼以外にそれを知るのは、阪神の監督の瀬川(橋爪功)と幸嗣を瀬川に紹介したマッサージ師の楊(國村準)だけ。楊に肩を治療してもらい、昔の剛速球を取り戻した幸嗣だったが、年齢と家族の将来のためにサラリーマンは辞められないと考えた。そこで瀬川は、彼に甲子園球場限定のパートタイムピッチャーになることを提案したのだった。しかしサラリーマンとの両立に疲れ自暴自棄になった幸嗣は、登録を抹消されてしまう。ちょうどその正体に気付いた妻(鶴田真由)は、簡単に野球を諦めようとする彼に「夢を諦めるな」と激怒。家族に後押しされ、幸嗣は再びマウンドに立ち、その覆面をとったのだった。元プロ野球選手が主演の本格的ベースボール・エンターテイメント。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "123", "タイトル": "水の女", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/bb/b4/c7/3c/94e5c3b1e4b0518988b022bf76a912b4_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/bb/b4/c7/3c/ca5206882b7501d59167f6ba65f109ed_640.jpg", "caption": ""}], "公開年月日": "2002年11月9日", "上映時間": "115 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "杉森秀則", "name_id": "1180", "職種": "監督"}, {"名前": "甲斐真樹", "id": "5676", "職種": "プロデューサー", "役割": ""}, {"名前": "根岸洋之", "id": "1179", "職種": "プロデューサー", "役割": ""}, {"名前": "杉森秀則", "id": "1180", "職種": "スタッフ", "役割": "脚本"}, {"名前": "町田博", "id": "293", "職種": "スタッフ", "役割": "撮影"}, {"名前": "菅野よう子", "id": "1181", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "UA", "id": "553", "役割": "清水涼"}, {"名前": "浅野忠信", "id": "37", "役割": "優作"}, {"名前": "小川眞由美", "id": "1182", "役割": "翠"}], "製作会社": ["アーティストフィルム", "日活"], "制作会社": [], "配給会社（国内）": [], "説明": "日本を代表する女性歌手のひとりUAの、初主演作。ギリシア自然哲学における宇宙の四元素、水火風地に登場人物たちをなぞらえて、物語は構成される。涼(UA)は、雨女と呼ばれている。実家は銭湯を営んでいるが、ある日父と婚約者とが、同時にこの世を去る。天涯孤独の身となった傷心の涼は旅に出て、森の奥で風のように自由に暮らすユキノ(HIKARU)と出会う。実家に戻ると見知らぬ男・優作(浅野忠信)が、家に上がりこんでいる。涼は優作を銭湯の釜場で働かないかと誘う。涼から「おかあちゃん」と呼ばれる翠(小川眞由美)はショッピングバッグ・レディである。街そのものを住居とする彼女は地の役割。こうやって揃った四元素が絡み合い、銭湯の高い煙突を中心に持つ、小宇宙を作り上げる。それは、幼い頃の涼の見た、記憶の中のまばゆい光景でもあった。しかし現実のその小宇宙は儚く消える。だが涼は、その孤独をもはや悲しむことはなかった。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "122", "タイトル": "木曜組曲", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/a5/c7/33/aa/cc41a589d757fc682468b5fef52438e9_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/a5/c7/33/aa/b263828905b5609488ba831f4e8c4553_640.jpg", "caption": ""}], "公開年月日": "2002年10月12日", "上映時間": "113 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "篠原哲雄", "name_id": "505", "職種": "監督"}, {"名前": "鈴木光", "id": "250", "職種": "プロデューサー", "役割": ""}, {"名前": "大森寿美男", "id": "506", "職種": "スタッフ", "役割": "脚本"}, {"名前": "恩田陸", "id": "1174", "職種": "スタッフ", "役割": "原作"}, {"名前": "高瀬比呂志", "id": "226", "職種": "スタッフ", "役割": "撮影"}, {"名前": "村山達哉", "id": "508", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "浅丘ルリ子", "id": "1175", "役割": "重松時子"}, {"名前": "鈴木京香", "id": "1176", "役割": "塩谷絵里子"}, {"名前": "原田美枝子", "id": "1177", "役割": "川渕静子"}], "製作会社": ["光和インターナショナル"], "制作会社": [], "配給会社（国内）": [], "説明": "日本を代表する女優たちの共演が話題となった篠原哲雄監督作。耽美派作家の巨匠、重松時子(浅丘ルリ子)が謎の薬物死を遂げて4年。毎年その命日が近づくと、彼女が死んだ日にその館に居合わせた5人の女たちが集い、彼女を偲んで宴を催していた。今年の宴は「皆様の罪を忘れないために、今日この場所に死者のための花を捧げます」というメッセージが添えられた花束が届けられるという不穏な幕開けとなった。それをきっかけに、それぞれ物書きを生業とする女たちは、遺書が発見されたために自殺と断定された時子の死について改めて推理を巡らせていく。当時時子が小説を書けなくなり、ある妄想に取りつかれていたことや、それぞれのみが知る彼女の秘密が明らかになるにつれ、推理は二転三転するが、最後に絵里子(鈴木京香)が驚くべき推理を展開。女たちはその話に唖然としつつも、それぞれが時子のことを文章にしようと心を決め、館を後にする。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "121", "タイトル": "夜を賭けて", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/3a/f7/db/f7/4780f8fe55760dcb3ba42964c3ffdbec_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/3a/f7/db/f7/a59509535a2595db163ddc9be27f2075_640.jpg", "caption": ""}], "公開年月日": "2002年11月30日", "上映時間": "133 分", "カテゴリー": ["劇映画", "長編"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "金守珍", "name_id": "1166", "職種": "監督"}, {"名前": "郭充良", "id": "1164", "職種": "プロデューサー", "役割": ""}, {"名前": "車勝宰", "id": "1165", "職種": "プロデューサー", "役割": ""}, {"名前": "丸山昇一", "id": "5315", "職種": "スタッフ", "役割": "脚本"}, {"名前": "梁石日", "id": "1168", "職種": "スタッフ", "役割": "原作"}, {"名前": "崔汀友", "id": "1169", "職種": "スタッフ", "役割": "撮影"}, {"名前": "朴保", "id": "1170", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "山本太郎", "id": "1171", "役割": "金義夫"}, {"名前": "風吹ジュン", "id": "1172", "役割": "タカシのオモニ"}, {"名前": "樹木希林", "id": "1173", "役割": "健一のオモニ"}], "製作会社": ["「夜をかけて」製作委員会", "アートン", "サイダス"], "制作会社": [], "配給会社（国内）": [], "説明": "役者としても活躍し、新宿梁山泊の演出家でもある金守珍監督による日韓合作映画。1958年、終戦前夜に爆撃によって焼き払われた大阪造兵廠の焼跡近くの朝鮮人集落に良雄(山本太郎)は帰ってくる。造兵廠の跡地には鉄屑がいまだ眠っていることを知った集落の人々は、警備が手薄になる夜の間に忍び込み、それを盗み出す計画を立てる。計画に成功し、喜ぶ人々の中で良雄は朝鮮戦争の孤児・初子(ユー・ヒュンギョン)を眼にする。盗みも回を重ねるに連れ、収穫も減り、警備も厳しくなる。そんな中、良雄の仲間が盗みの現場を見つけた警官を過って殺してしまう。それを契機に警察の追及は厳しくなり、集落を立ち退かねばならない日も迫る。ある日、初子は母と共にこの集落を出ることになり、良雄に対する秘めていた思いを打ち明ける。抱き合う二人。そこに大勢の警察が強制的に立ち退きを迫ってやってくる。混乱の中、どこからかあがった火が集落を包み、全てを焼き尽くしてしまう。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "120", "タイトル": "ロックンロールミシン", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/7f/5b/a6/8a/3b3fab936911ee68dc15f602c757bb51_640.jpg", "caption": "(c) 2001 SPACE SHOWER NETWORKS INC./ GAGA Communications, Inc./ 東映ビデオ株式会社"}, {"url": "//jfdb.jp/data/photo/movie/7f/5b/a6/8a/830ea419560bfae2bb17925f34c7cafb_640.jpg", "caption": "(c) 2001 SPACE SHOWER NETWORKS INC./ GAGA Communications, Inc./ 東映ビデオ株式会社"}], "公開年月日": "2002年9月28日", "上映時間": "120 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "行定勲", "name_id": "444", "職種": "監督"}, {"名前": "長谷川真澄", "id": "993", "職種": "プロデューサー", "役割": ""}, {"名前": "鈴木裕光", "id": "1156", "職種": "プロデューサー", "役割": ""}, {"名前": "松井俊之", "id": "1157", "職種": "プロデューサー", "役割": ""}, {"名前": "佐藤信介", "id": "1091", "職種": "スタッフ", "役割": "脚本"}, {"名前": "行定勲", "id": "444", "職種": "スタッフ", "役割": "脚本"}, {"名前": "鈴木清剛", "id": "1158", "職種": "スタッフ", "役割": "原作"}, {"名前": "福本淳", "id": "446", "職種": "スタッフ", "役割": "撮影"}, {"名前": "めいな co.", "id": "5354", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "池内博之", "id": "1160", "役割": "中山凌一"}, {"名前": "りょう", "id": "1111", "役割": "椿めぐみ"}, {"名前": "加瀬亮", "id": "8710", "役割": "近田賢司"}], "製作会社": ["スペースシャワーネットワーク", "ギャガ・コミュニケーションズ", "東映ビデオ"], "制作会社": [], "配給会社（国内）": [], "説明": "2001年日本アカデミー賞監督賞、脚本賞、撮影賞などを獲得した『GO』の次回作として行定勲監督が作り上げたのは、夢の実現と挫折を味わう若者たちの物語。何となく会社勤めを続け、彼女とも適当に付き合いつつ物足りない日々を送っている賢司(水橋研二)は、高校時代の同級生・凌一(池内博之)と再会する。凌一は仲間たちとデザイナーズ・ブランドを立ち上げようとしているところだと言う。ある日、彼らの「工房」を訪ねた賢司は、仲間たちの自由な雰囲気に魅せられてしまう。彼らとの関わりが深まるにつれ、賢司は自分の暮らしてきた社会にますます馴染めなくなり、ある日部長を殴り、会社を辞める。正式に凌一たちの仲間入りをした賢司の、新鮮で、自由な生活が始まる。しかしそれは長続きしなかった。彼らの作った服は、思ったほど売れなかったのだ。資金不足、疲労感、アイディアの枯渇といった悪循環の中で、彼らそれぞれの生きる道を見つけるための模索が始まる。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "119", "タイトル": "笑う蛙", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/25/86/08/aa/a57d7d537ee115a4ce20807de23d92cb_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/25/86/08/aa/b0b2b29d5dad0f8060e96d6426944677_640.jpg", "caption": ""}], "公開年月日": "2002年7月6日", "上映時間": "96 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "-", "スタッフ": [{"name": "平山秀幸", "name_id": "172", "職種": "監督"}, {"名前": "福島聡司", "id": "1150", "職種": "プロデューサー", "役割": ""}, {"名前": "久保田傑", "id": "189", "職種": "プロデューサー", "役割": ""}, {"名前": "成島出", "id": "484", "職種": "スタッフ", "役割": "脚本"}, {"名前": "藤田宜永", "id": "1151", "職種": "スタッフ", "役割": "原作"}, {"名前": "柴崎幸三", "id": "518", "職種": "スタッフ", "役割": "撮影"}, {"名前": "村山竜二", "id": "1152", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "長塚京三", "id": "1153", "役割": "逸平"}, {"名前": "大塚寧々", "id": "84", "役割": "涼子"}, {"名前": "雪村いづみ", "id": "3943", "役割": "涼子の母"}], "製作会社": ["バップ", "衛星劇場", "ジャパンホームビデオ", "博報堂", "日販", "ワコー", "オフィス・シロウズ"], "制作会社": [], "配給会社（国内）": [], "説明": "倉沢逸平(長塚京三)が勤務先の銀行の金を横領し、失踪した。妻の涼子(大塚寧々)は、世間の目から逃れ、実家の別荘で静かに暮らしている。そこへ逃亡生活に疲れた逸平が現れ、二人は鉢合せする。最初は自首を勧めていた涼子だったが、離婚届に判を押すことを条件に、一週間だけ匿ってもいいと提案。涼子は新しい恋人の吉住(國村準)に再婚を望まれていたのだ。しぶしぶそれを承諾し、納戸で隠れ暮らすことになった逸平だが、涼子は平気で来客を家に上げ、恋人と情事を行う。壁の隙間からそれを目にした逸平は、涼子に対する欲望を取り戻す。数日後、逸平らしき男の死体が見つかったと刑事が報告に訪れる。それが逸平ではないことを知るのは、彼ら夫婦だけ。しかし、涼子と別れたくないから自首をすると言う逸平に、涼子は吉住との子供を身ごもったと嘘をつき、彼を逃がすのだった。『愛を乞う人』で数々の賞を受賞した平山秀幸が奇妙な男女関係を描く。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "118", "タイトル": "13階段", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/89/89/b3/bd/cc6722db97e9b420b23ece2d6f65b083_640.jpg", "caption": ""}, {"url": "//jfdb.jp/data/photo/movie/89/89/b3/bd/a21ed09b3d4bcc0ddb80cb0cf83b6242_640.jpg", "caption": ""}], "公開年月日": "2003年2月8日", "上映時間": "122 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "35mm", "スタッフ": [{"name": "長澤雅彦", "name_id": "1141", "職種": "監督"}, {"名前": "角谷優", "id": "1140", "職種": "プロデューサー", "役割": ""}, {"名前": "森下直", "id": "1142", "職種": "スタッフ", "役割": "脚本"}, {"名前": "高野和明", "id": "1143", "職種": "スタッフ", "役割": "原作"}, {"名前": "藤澤順一 (J.S.C)", "id": "305", "職種": "スタッフ", "役割": "撮影"}, {"名前": "ニック・ウッド", "id": "1144", "職種": "スタッフ", "役割": "音楽"}], "出演者": [{"名前": "反町隆史", "id": "1145", "役割": "三上純一"}, {"名前": "山﨑努", "id": "1146", "役割": "南郷正二"}, {"名前": "笑福亭鶴瓶", "id": "1147", "役割": "杉浦弁護士"}], "製作会社": ["フジテレビジョン", "東宝", "ポニーキャニオン", "IMAGICA"], "制作会社": [], "配給会社（国内）": [], "説明": "三上純一(反町隆史)はかつて喧嘩相手を誤って死なせ、刑に服していた。仮釈放された純一は、刑務所の刑務官・南郷(山崎努)に、樹原という死刑確定囚の冤罪を晴らす調査に誘われる。殺人容疑をかけられた樹原は、事件直後の交通事故で犯行時刻の記憶を無くし犯行を否認することもできずにいた。犯行当時に「階段を昇っていた」という樹原の僅かな記憶だけを手がかりに、純一と南郷は事件現場に赴き、その階段を探す。ついにふたりはその場所を見つけるが、そこにあった斧からは何と純一の指紋が採取される。その場所は偶然にも純一が殺してしまった男の故郷でもあり、その男の父親が純一への復讐のために証拠を偽装したのだ。その後、同じ場所から見つかった被害者の通帳から被害者に脅されていた真犯人の名前が浮かびあがり、樹原は救われた。", "公式サイト": [], "映画祭・受賞歴": []}
