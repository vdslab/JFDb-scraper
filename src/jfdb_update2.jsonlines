{"id": "214", "タイトル": "リバイバル・ブルース", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/54/f6/47/44/db313708fe00a796803014f61cb4bedc_640.jpg", "caption": "(C) ZUNO Films & ZERO Pictures"}, {"url": "//jfdb.jp/data/photo/movie/54/f6/47/44/d5a5664b874026bf3454d9d3943a1629_640.jpg", "caption": "(C)ZUNO Films &amp; ZERO Pictures"}], "公開年月日": "2004年10月9日", "上映時間": "114 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "35mm", "スタッフ": [{"name": "クロード・ガニオン", "name_id": "1874", "職種": "監督"}, {"名前": "ユリ・ヨシムラ・ガニオン", "id": "1870", "職種": "プロデューサー", "役割": ""}, {"名前": "サミュエル・ガニオン", "id": "1871", "職種": "プロデューサー", "役割": ""}, {"名前": "奥田瑛二", "id": "1872", "職種": "プロデューサー", "役割": ""}, {"名前": "クロード・ガニオン", "id": "1874", "職種": "スタッフ", "役割": "脚本"}, {"名前": "クロード・ガニオン", "id": "1874", "職種": "スタッフ", "役割": "撮影"}, {"名前": "クロード・ガニオン", "id": "1874", "職種": "スタッフ", "役割": "編集"}, {"名前": "塩次伸二", "id": "1875", "職種": "スタッフ", "役割": "音楽"}, {"名前": "塩次伸二", "id": "1875", "職種": "スタッフ", "役割": ""}, {"名前": "塩次伸二", "id": "1875", "職種": "スタッフ", "役割": ""}], "出演者": [{"名前": "内藤剛志", "id": "1877", "役割": "健"}, {"名前": "奥田瑛二", "id": "1872", "役割": "洋介"}, {"名前": "桃井かおり", "id": "1878", "役割": "加代"}], "製作会社": ["ゼロ・ピクチュアズ", "ズノ・フィルムス"], "制作会社": [], "配給会社（国内）": [], "説明": "出張で沖縄を訪れた健(内藤剛志)は、ずっと以前にバンド仲間だった洋介(奥田瑛二)の店を訪ねる。だが洋介の態度は冷たかった。ふたりの間には26年前のバンド解散について、まだわだかまりが残っていたのだった。バンドの再結成を持ちかける健の前で洋介は急に倒れ、彼が末期ガンに蝕まれていることが判明する。健は、病身の洋介を説得し、ボーカルだった加代(桃井かおり)と共に、かつてのバンドを一夜だけ復活させる。その後、健はホテル住まいだった洋介と彼の妻・静香を自宅に招き、献身的な介護を始めるのだが、洋介の容態は重かった。友情を取り戻した途端の親友の死。寂しさをこらえ、健は静香を沖縄へ見送った。監督のクロード・ガニオンは79年に日本で製作された初監督作品『Keiko』で注目されたカナダ人。本作は、現在カナダを中心に活動しているガニオンが再び日本で撮った作品。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "213", "タイトル": "理由", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/9c/83/1e/9e/4469fa023316785c308cffdc26f2f311_640.jpg", "caption": "(C) 2004 WOWOW / PSC"}], "公開年月日": "2004年12月18日", "上映時間": "160 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "35mm", "スタッフ": [{"name": "大林宣彦", "name_id": "1862", "職種": "監督"}, {"名前": "戸田幸宏", "id": "1859", "職種": "プロデューサー", "役割": ""}, {"名前": "大林恭子", "id": "1860", "職種": "プロデューサー", "役割": ""}, {"名前": "山崎輝道", "id": "1861", "職種": "プロデューサー", "役割": ""}, {"名前": "宮部みゆき", "id": "1863", "職種": "スタッフ", "役割": "原作"}, {"名前": "大林宣彦", "id": "1862", "職種": "スタッフ", "役割": "脚本"}, {"名前": "石森史郎", "id": "1864", "職種": "スタッフ", "役割": "脚本"}], "出演者": [{"名前": "岸部一徳", "id": "258", "役割": "佐野利明"}, {"名前": "久本雅美", "id": "1865", "役割": "葛西美枝子"}, {"名前": "古手川祐子", "id": "1866", "役割": "秋吉勝子"}, {"名前": "風吹ジュン", "id": "1172", "役割": "小糸静子"}, {"名前": "柄本明", "id": "296", "役割": "片倉義文"}, {"名前": "小林聡美", "id": "1867", "役割": "北畠敦子"}], "製作会社": ["WOWOW", "PSC ／大林宣彦事務所"], "制作会社": [], "配給会社（国内）": [], "説明": "ある高層マンションの20階の一室で3体の惨殺死体、そして、その一室から落下したと思われる男性の遺体が地上で発見される。住人台帳では、その部屋には小糸信治とその家族が住んでいることになっていた。しかし、発見された遺体は小糸一家のものではない。小糸家はローンが支払えず、夜逃げしていたのだ。被害者たちは、小糸が新たな買い手に部屋を明け渡さないために住まわせていた砂川一家だった。その部屋を競売で落札した石田という男が行方をくらませたため、彼に嫌疑がかかる。ところが砂川の妻だという女性が名乗りを上げ、殺された4人が砂川一家ではなく、他人同士の疑似家族であったことが判明。事件はさらに謎を深めるが、多くの人々の証言から次第にその真相が浮かび上がっていく。『時をかける少女』『ふたり』の監督である大林宣彦が、100人を超える人々の証言によって構成される原作を忠実に再現している。", "公式サイト": [], "映画祭・受賞歴": []}
{"id": "212", "タイトル": "レディ・ジョーカー", "予告動画": "", "image": [{"url": "//jfdb.jp/data/photo/movie/f1/ff/71/a8/df5aa39607cf8582a6b400d895b9a6ce_640.jpg", "caption": "(C)2004レディ・ジョーカー製作委員会"}], "公開年月日": "2004年12月11日", "上映時間": "121 分", "カテゴリー": ["劇映画"], "カラー": "Color", "フォーマット": "35mm", "スタッフ": [{"name": "平山秀幸", "name_id": "172", "職種": "監督"}, {"名前": "石原清行", "id": "1854", "職種": "プロデューサー", "役割": ""}, {"名前": "鄭義信", "id": "1320", "職種": "スタッフ", "役割": "脚本"}, {"名前": "高村薫", "id": "1855", "職種": "スタッフ", "役割": "原作"}], "出演者": [{"名前": "渡哲也", "id": "1856", "役割": "物井清三"}, {"名前": "長塚京三", "id": "1153", "役割": "城山恭介"}, {"名前": "徳重聡", "id": "1857", "役割": "合田雄一郎"}], "製作会社": ["日活", "東映", "毎日新聞社", "テレビ朝日", "AOI Pro.", "スポーツニッポン", "日販"], "制作会社": [], "配給会社（国内）": [], "説明": "『愛を乞うひと』『魔界転生』の監督として知られる平山秀幸がメガホンを取った社会派サスペンス映画。原作は、1984年に日本中を震撼させた菓子製品への毒物混入事件、「グリコ・森永事件」に着想を得て書かれたもの。業界最大手のビール会社である日之出ビールの社長が誘拐された。「レディ・ジョーカー」と名乗る犯人グループは社長を解放する際、20億円を要求、「人質は350万キロリットルのビールだ」と告げる。日之出ビール社長は自社製品の安全を守るために、犯人たちの指示に従って警察に嘘をつき、20億円を払うことを決める。社長の警護についた刑事の合田(徳重聡)は、日之出ビールが警察に隠れて犯人と接触していることを知り、徐々に「レディ・ジョーカー」の実像に近づいていく。意外なことに、「レディ・ジョーカー」は小さな薬店を営む物井(渡哲也)を始め、社会の片隅に生きる名もない男たちだった。", "公式サイト": [], "映画祭・受賞歴": []}
